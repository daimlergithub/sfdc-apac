    <validationRules>
        <fullName>NttDataEpa__Integration_Only_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(NttDataEpa__Integration_Only__c) &amp;&amp; ( NttDataEpa__Integration_Only__c || ISBLANK( NttDataEpa__Picklist_Value__c ))</errorConditionFormula>
        <errorMessage>If Integration Only is not checked, you have to define a Picklist Value, otherwise you may or may not define a Picklist value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NttDataEpa__Integration_Value_Syntax</fullName>
        <active>true</active>
        <description>Checks the integration value syntax.</description>
        <errorConditionFormula>NOT(REGEX( Name , &quot;[A-Z0-9_]+&quot;))</errorConditionFormula>
        <errorMessage>The Integration Value may consist of latin upper cases, arabic numbers and underlines only.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NttDataEpa__Validate_External_Key_Syntax</fullName>
        <active>true</active>
        <description>Check that the syntax of the External Key is correct.</description>
        <errorConditionFormula>NOT(BEGINS(NttDataEpa__External_Key__c, &apos;IP-&apos;)) || NOT(ISNUMBER( RIGHT(NttDataEpa__External_Key__c, 6)))</errorConditionFormula>
        <errorMessage>The syntax of the external key must match IP-{000000}.</errorMessage>
    </validationRules>
