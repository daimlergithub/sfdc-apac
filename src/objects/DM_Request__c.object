        <errorConditionFormula>AND(Expected_DM_Material3__c != Null,Expected_DM_Material3__r.Stock__c &lt;=0,$Profile.Name != &apos;IntegrationAPI&apos;,ISPICKVAL($Profile.UserType, &apos;Standard&apos;), ISPICKVAL($User.Market__c,&apos;KR&apos;))</errorConditionFormula>
