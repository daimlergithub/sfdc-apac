    <validationRules>
        <fullName>Campaign_Package_cannot_be_empty</fullName>
        <active>true</active>
        <description>To prevent adding Campaign Package without any Package Items</description>
        <errorConditionFormula>Campaign_Package__r.Package_Items__c =0</errorConditionFormula>
        <errorDisplayField>Campaign_Package__c</errorDisplayField>
        <errorMessage>You can&apos;t add a Campaign Package without any Package Items.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>List_Maintenance_lock</fullName>
        <active>true</active>
        <errorConditionFormula>AND(($Profile.Name != &apos;IntegrationAPI&apos;),( MD__c = &apos;JP&apos;),Campaign__r.Consolidated_List_Completed__c = TRUE)</errorConditionFormula>
        <errorMessage>List Maintenance is Completed,You Cant Edit And Create</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Session_Start_Date_is_less_than_End_Date</fullName>
        <active>true</active>
        <errorConditionFormula>Session_Start_Date__c  &gt;  Session_End_Date__c</errorConditionFormula>
        <errorMessage>Session Start Date must be less than or equal to Session End Date</errorMessage>
    </validationRules>
