    <webLinks>
        <fullName>Complete</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Complete</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/32.0/connection.js&quot;)} 

if({!Participating_Dealer__c.Approve_the_List__c  } == false) 
{ 
var c = new sforce.SObject(&quot;Participating_Dealer__c&quot;); 
c.id = &quot;{!Participating_Dealer__c.Id}&quot;; 
c.Approve_the_List__c   = true; 
result = sforce.connection.update([c]); 
if ( result[0].getBoolean( &quot;success&quot; ) ) 
{ 
window.location.reload(); 
} 
else 
{ 
alert( result[0].errors.message); 
} 
} 
else 
{ 
alert(&quot;Already True&quot;); 
}</url>
