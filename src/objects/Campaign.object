    </validationRules>
    <validationRules>
        <fullName>CAS_Marketing_Active</fullName>
        <active>true</active>
        <errorConditionFormula>AND(RecordType.DeveloperName = &apos;CAS_Marketing_Campaign&apos;,ISNEW(),IsActive = True,UPPER(MD__c) = &apos;JP&apos;,$Profile.Name != &apos;IntegrationAPI&apos; )</errorConditionFormula>
