    <validationRules>
        <fullName>CamExecution_Staus_Change_form_Completed</fullName>
        <active>true</active>
        <description>If campaign execution is &apos;Completed&apos; then it can&apos; t be changed to &apos;Planning&apos; or &apos;Segmentation&apos; or &apos;Execution&apos; or &apos;Response collection&apos;</description>
        <errorConditionFormula>AND( 
ISPICKVAL(
PRIORVALUE(Child_Campaign_Status__c),&apos;Completed&apos;),
     OR(ISPICKVAL(Child_Campaign_Status__c, &apos;Planning&apos;), 
      ISPICKVAL(Child_Campaign_Status__c, &apos;Segmentation&apos;),
      ISPICKVAL(Child_Campaign_Status__c, &apos;Execution&apos;),
  ISPICKVAL(Child_Campaign_Status__c, &apos;Response collection&apos;) ,ISPICKVAL(Child_Campaign_Status__c, &apos;&apos;)

     ),
     MD__c = &apos;JP&apos;, 
     $Profile.Name != &apos;IntegrationAPI&apos;,
       RecordType.DeveloperName = &apos;Campaign_Executiono&apos; 
)</errorConditionFormula>
        <errorMessage>Status cannot be changed when it is &apos;Completed&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CamExecution_Staus_Change_form_Execution</fullName>
