      ISPICKVAL(Child_Campaign_Status__c, &apos;Segmentation&apos;),
      ISPICKVAL(Child_Campaign_Status__c, &apos;&apos;)
     ),
     MD__c = &apos;JP&apos;, 
     $Profile.Name != &apos;IntegrationAPI&apos;,
       RecordType.DeveloperName = &apos;Campaign_Executiono&apos; 
)</errorConditionFormula>
        <errorMessage>Status cannot be changed from &apos;Exection&apos; to &apos;Planning or Segmentation&apos;</errorMessage>
    </validationRules>
