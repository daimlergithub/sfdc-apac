        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT("/soap/ajax/19.0/connection.js")}; 
{!REQUIRESCRIPT("/soap/ajax/19.0/apex.js")}; 
{!REQUIRESCRIPT("/support/console/27.0/integration.js")} 
var idList = {!GETRECORDIDS($ObjectType.QC__c)}; 
var retURL = window.location.toString(); 
var proceed = true; 
if(idList.length &lt;= 0){ 
alert('Please select record(s).'); 
proceed = false; 
} 
if ((navigator.userAgent.indexOf('MSIE')) &gt;= 0) { 
if(idList.length &gt; 100){ 
alert('Only maximum of 100 records can be selected.'); 
proceed = false; 
} 
} 
if(proceed){ 
if (sforce.console.isInConsole() &amp;&amp; typeof(srcUp)!='undefined') { 
srcUp("/apex/QCMassApproval?isdtp=vw&amp;objIds=" + idList + "&amp;retURL=" + retURL); 
} else { 
window.location.href = "/apex/QCMassApproval?objIds=" + idList + "&amp;retURL=" + retURL; 
} 

}</url>
