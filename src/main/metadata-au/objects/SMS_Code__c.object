        <active>true</active>
        <label>Register Campaign</label>
    </recordTypes>
    <searchLayouts/>
    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>No_Next_SMS_when_Send_Closure_SMS</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISBLANK(NextSMSTemplate__c)),SendClosureSMS__c=TRUE)</errorConditionFormula>
        <errorMessage>You can not choose "Next SMS" and "Send Closure SMS" together!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Template_Closure_SMS_Required</fullName>
        <active>true</active>
        <errorConditionFormula>(RecordType.Name == 'Generate OB Task' || RecordType.Name == 'Register Campaign') &amp;&amp; SendClosureSMS__c &amp;&amp; Template__c != null &amp;&amp; ISBLANK(Template__r.Closure_SMS__c)</errorConditionFormula>
        <errorMessage>Your must enter a Closure SMS value for your template if the Send Closure SMS is checked.</errorMessage>
    </validationRules>
