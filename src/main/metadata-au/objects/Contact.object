    <webLinks>
        <fullName>Active_Permission_Set</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Active Permission Set</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/apex.js&quot;)}

var newRecords = [];
var userInfo = sforce.connection.query(&quot;select Id from User where ContactId = &apos;&quot;+&apos;{!Contact.Id}&apos;+&quot;&apos;&quot;);

if(userInfo.records != null) { 
  var c = new sforce.SObject(&quot;User&quot;); 
  c.id =userInfo.records.Id;
  c.Active_Permission_Set__c = true;
  newRecords.push(c); 
  result = sforce.connection.update(newRecords);
  if(result[0].getBoolean(&quot;success&quot;)){
      window.location.reload();
  } else {
      alert(result[0].errors.message);
  }
}else if(userInfo.records == null) {
  alert(&quot;Users who are not associated with the current contact is created!&quot;);
}</url>
