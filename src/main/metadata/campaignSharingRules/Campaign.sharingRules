    <criteriaBasedRules>
        <fullName>Allow_Only_to_special_users</fullName>
        <sharedTo>
            <role>Manager</role>
        </sharedTo>
        <criteriaItems>
            <field>RecordTypeId</field>
            <operation>equals</operation>
            <value>Central Marketing Campaign</value>
        </criteriaItems>
        <criteriaItems>
            <field>MD__c</field>
            <operation>equals</operation>
            <value>KR</value>
        </criteriaItems>
        <criteriaItems>
            <field>Status</field>
            <operation>equals</operation>
            <value>Published</value>
        </criteriaItems>
        <criteriaItems>
            <field>Status</field>
            <operation>equals</operation>
            <value>Closed</value>
        </criteriaItems>
        <booleanFilter>1 AND 2 AND (3 OR 4)</booleanFilter>
        <campaignAccessLevel>Edit</campaignAccessLevel>
        <name>Allow Only to special users</name>
    </criteriaBasedRules>
