                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Repair voucher</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Extend Warranty</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Others</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
    </fields>
