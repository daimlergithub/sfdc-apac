    <fields>
        <fullName>R_WithSettlement__c</fullName>
        <externalId>false</externalId>
        <formula>if( ISPICKVAL(Settlement_Type__c ,'No Settlement Require'),0,1)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
