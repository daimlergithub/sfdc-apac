        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>Settlement Managements</pluralLabel>
    <searchLayouts/>
    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>Other_Settlement_Type_Mandatory</fullName>
        <active>true</active>
        <description>When Settlement Type is "Others", "Other Settlement Type" field should be mandatory.</description>
        <errorConditionFormula>ISPICKVAL( Settlement_Type__c , 'Others') &amp;&amp;  ISBLANK( Other_Settlement_Type__c )</errorConditionFormula>
        <errorMessage>Please fill in Other Settlement Type.</errorMessage>
    </validationRules>
