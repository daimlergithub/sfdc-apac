        <unique>false</unique>
    </fields>
    <fields>
        <fullName>R_Without_Settlement__c</fullName>
        <externalId>false</externalId>
        <formula>if( ISPICKVAL(Settlement_Type__c ,'No Settlement Require'),1,0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Without Settlement</label>
