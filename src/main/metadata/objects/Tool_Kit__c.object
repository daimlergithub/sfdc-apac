{!REQUIRESCRIPT("/support/console/27.0/integration.js")} 
var retURLs = window.location.toString();
var retURL = retURLs.split("com",1);
var today = '{!Tool_Kit__c.Today__c}'.split(" ",1);
var tkId = '{!Tool_Kit__c.Id}';
window.location.href = "/apex/ToolKitSelectContent?toolkitId=" + tkId + "&amp;url=" + retURL + "&amp;today=" +today;</url>
