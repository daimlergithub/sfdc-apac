    <validationRules>
        <fullName>Unlock_Application</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
     NOT(
          OR(ISPICKVAL( Apply_Status__c , 'Planned'), 
                 ISPICKVAL( Apply_Status__c , 'Need Modify'),AND( RecordType.DeveloperName == 'smart_Event' ,
                 ISPICKVAL( Apply_Status__c , 'Tool Kit Finished'))
                )
             ),
     OR(    ISCHANGED(Brand__c),
            ISCHANGED(Ad_Color__c),
            ISCHANGED(Ad_Copy__c),
            ISCHANGED(Ad_Format__c),
            ISCHANGED(Ad_Size_Height__c),
            ISCHANGED(Ad_Size_Width__c),
            ISCHANGED(Background__c),
            ISCHANGED(Ad_Banner__c),
            ISCHANGED(Bleed_Size_Height__c),
            ISCHANGED(Bleed_Size_Width__c),
            ISCHANGED(Comments__c),
            ISCHANGED(Dealer_Name__c),
            ISCHANGED(Deliver_Date__c),
            ISCHANGED(Event_Director__c),
            ISCHANGED(Event_Kit_Name__c),
            ISCHANGED(Event_Participant__c),
            ISCHANGED(Event_Type__c),
            ISCHANGED(Location__c),
            ISCHANGED(Location_Size__c),
            ISCHANGED(Media_Kit_Name__c),
            ISCHANGED(Media_Name__c),
            ISCHANGED(Media_Type__c),
            ISCHANGED(Plan_End_Date__c),
            ISCHANGED(Plan_Expense__c),
            ISCHANGED(Plan_Start_Date__c),
            ISCHANGED(Purpose__c),
			ISCHANGED(Request__c),
			ISCHANGED(Showroom__c),
            ISCHANGED(Name))
)</errorConditionFormula>
        <errorMessage>You can't update Tool Kit information with current apply status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Unlock_Application_Feedback</fullName>
        <active>true</active>
        <errorConditionFormula>OR(AND( OR(ISPICKVAL( Feedback_Status__c , 'Confirmed'),
        ISPICKVAL( Feedback_Status__c , 'Approved')),
     OR(ISCHANGED( Feedback_Comments__c ),  ISCHANGED( Actual_Start_Date__c ) ,ISCHANGED( Actual_End_Date__c ))),
   AND(NOT(ISPICKVAL(  Apply_Status__c , 'Tool Kit Finished')),
   OR(ISCHANGED( Feedback_Comments__c ),ISCHANGED( Actual_Start_Date__c ) ,ISCHANGED( Actual_End_Date__c )))
)</errorConditionFormula>
        <errorDisplayField>Actual_End_Date__c</errorDisplayField>
        <errorMessage>You can't update Feedback information with current feedback status or apply status.</errorMessage>
