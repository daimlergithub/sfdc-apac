        <fullName>Type__c</fullName>
        <externalId>false</externalId>
        <label>Type</label>
        <picklist>
            <picklistValues>
                <fullName>Single Select--Vertical</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Single Select--Horizontal</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Multi-Select--Vertical</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Free Text</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
    </fields>
    <label>Survey Question</label>
    <listViews>
        <fullName>All</fullName>
        <columns>NAME</columns>
        <columns>OrderNumber__c</columns>
        <columns>Question__c</columns>
        <columns>Survey__c</columns>
        <columns>LAST_UPDATE</columns>
        <filterScope>Everything</filterScope>
        <label>All</label>
        <language>ja</language>
        <sharedTo>
            <roleAndSubordinatesInternal>Central</roleAndSubordinatesInternal>
        </sharedTo>
    </listViews>
    <listViews>
        <fullName>MBK_Survey_Questions</fullName>
        <columns>NAME</columns>
        <columns>OrderNumber__c</columns>
        <columns>Question__c</columns>
        <columns>Survey__c</columns>
        <columns>LAST_UPDATE</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>MD__c</field>
            <operation>equals</operation>
            <value>KR</value>
        </filters>
        <label>MBK Survey Questions</label>
        <language>en_US</language>
        <sharedTo>
            <roleAndSubordinatesInternal>MBK_Management</roleAndSubordinatesInternal>
        </sharedTo>
    </listViews>
