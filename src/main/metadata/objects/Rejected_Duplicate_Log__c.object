        <description>Requirement: US-DD-002
CreateBy: Mouse Liu
CreatedDate: 2013-06-08</description>
        <externalId>false</externalId>
        <label>Master Account</label>
        <referenceTo>Account</referenceTo>
        <relationshipLabel>Rejected Duplicate Logs</relationshipLabel>
        <relationshipName>Rejected_Duplicate_Logs</relationshipName>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>R_checkRecordID__c</fullName>
        <externalId>false</externalId>
        <formula>IF(Id!=NULL,1,0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>checkRecordID</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
