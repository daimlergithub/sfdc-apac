    <validationRules>
        <fullName>Campaign_Package_cannot_be_empty</fullName>
        <active>true</active>
        <description>To prevent adding Campaign Package without any Package Items</description>
        <errorConditionFormula>Campaign_Package__r.Package_Items__c =0</errorConditionFormula>
        <errorDisplayField>Campaign_Package__c</errorDisplayField>
        <errorMessage>You can't add a Campaign Package without any Package Items.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>List_Maintenance_lock</fullName>
        <active>true</active>
        <errorConditionFormula>AND(($Profile.Name != 'IntegrationAPI'),( MD__c = 'JP'),Campaign__r.Consolidated_List_Completed__c = TRUE)</errorConditionFormula>
        <errorMessage>List Maintenance is Completed,You Cant Edit And Create</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Session_Start_Date_is_less_than_End_Date</fullName>
        <active>true</active>
        <errorConditionFormula>Session_Start_Date__c  &gt;  Session_End_Date__c</errorConditionFormula>
        <errorMessage>Session Start Date must be less than or equal to Session End Date</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Complete</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Complete</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT("/soap/ajax/32.0/connection.js")} 

if({!Participating_Dealer__c.Approve_the_List__c  } == false) 
{ 
var c = new sforce.SObject("Participating_Dealer__c"); 
c.id = "{!Participating_Dealer__c.Id}"; 
c.Approve_the_List__c   = true; 
result = sforce.connection.update([c]); 
if ( result[0].getBoolean( "success" ) ) 
{ 
window.location.reload(); 
} 
else 
{ 
alert( result[0].errors.message); 
} 
} 
else 
{ 
alert("Already True"); 
}</url>
    </webLinks>
