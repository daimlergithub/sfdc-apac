        <url>{!REQUIRESCRIPT("/soap/ajax/19.0/connection.js")};

var questions = sforce.connection.query("SELECT ID FROM Survey_Question__c WHERE Survey__c = '{!Survey__c.Id}'");
var records = questions.getArray("records");
if (records.length&gt;0) {
window.open("/apex/SubmitSurvey?surveyID={!Survey__c.Id}&amp;type=preview","newwindow","height=300,width=500,top=200,left=750,scrollbars=yes");
