        <externalId>false</externalId>
        <formula>IF( Points__c&gt;= 30, IMAGE( $Label.Url_for_Rug_max_static ,"Hot"),
IF ( Points__c&gt;=20 &amp;&amp; Like_Count__c&lt;10, IMAGE( $Label.Url_for_Rug_Med_static, "Medium"),
IMAGE($Label.Url_for_Rug_Low_static, "Low")))</formula>
