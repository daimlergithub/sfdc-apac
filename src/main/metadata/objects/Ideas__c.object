    <validationRules>
        <fullName>Honorary_profile_and_status_check</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Status__c,'Taken up for product roadmap') &amp;&amp;  $Profile.Name=='Honorary Member' &amp;&amp;  Not(ISNEW())</errorConditionFormula>
        <errorMessage>You dont have access to select this picklist value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>New_record_status_should_be_open</fullName>
        <active>true</active>
        <errorConditionFormula>ISNEW()  &amp;&amp; NOT(ISPICKVAL(Status__c, 'open'))</errorConditionFormula>
        <errorMessage>Idea should be in "Open" status for new idea creation</errorMessage>
    </validationRules>
