    <sharingModel>Private</sharingModel>
    <validationRules>
        <fullName>Approevr_List_is_mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(ISPICKVAL(WorkFlow_Group__c,"")),
ISBLANK(Approver_List__c)
)</errorConditionFormula>
        <errorDisplayField>Approver_List__c</errorDisplayField>
        <errorMessage>Please select atleast one Approver.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Do_Not_Modify_News_Details</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL(Approval_Status__c ,'Pending'), 
OR (ISCHANGED(Name),ISCHANGED(Title__c),ISCHANGED(Description__c),ISCHANGED(Approver_List__c),ISCHANGED(Need_Approval__c),ISCHANGED(Category_Type__c),ISCHANGED(Remarks__c),ISCHANGED(Save_As_Draft__c),ISCHANGED(Share_With_Groups__c),ISCHANGED(WorkFlow_Group__c), 
ISPICKVAL(Document_Status__c,'Expired')) 

)</errorConditionFormula>
        <errorMessage>You are not allowed to change the News details in this stage</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Do_Not_Modify_News_EndDate</fullName>
        <active>true</active>
        <description>Do_Not_Modify_News_EndDate if it is not required. Only can be changed if it is less than start date</description>
        <errorConditionFormula>AND( 
NOT($Profile.Name="System Administrator"), 
ISPICKVAL(Approval_Status__c ,'Pending'), 
AND(ISCHANGED(Expiration_Date__c),Start_Date__c &gt;= Today() , NOT(ISCHANGED(Start_Date__c))) 
)</errorConditionFormula>
        <errorMessage>You are not allowed to change the News details in this stage</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EndDate_should_be_greater_than_StartDate</fullName>
        <active>true</active>
        <description>News End Date should be greater than Start Date.</description>
        <errorConditionFormula>Expiration_Date__c &lt; Start_Date__c</errorConditionFormula>
        <errorDisplayField>Expiration_Date__c</errorDisplayField>
        <errorMessage>News End Date should be greater than or equal to Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Date_Should_be_Future_Date</fullName>
        <active>true</active>
        <description>News Start Date Should be Future Date.</description>
        <errorConditionFormula>Start_Date__c  &lt; Today()</errorConditionFormula>
        <errorDisplayField>Start_Date__c</errorDisplayField>
        <errorMessage>News Start Date Should be Today or Future Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Workflow_Group_is_Mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
Need_Approval__c == True,
ISPICKVAL(WorkFlow_Group__c,"")
)</errorConditionFormula>
        <errorDisplayField>WorkFlow_Group__c</errorDisplayField>
        <errorMessage>Please select Workflow Group.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>needforapproval_rule</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
Save_As_Draft__c == True,
 Need_Approval__c == True
)</errorConditionFormula>
        <errorDisplayField>Need_Approval__c</errorDisplayField>
        <errorMessage>In order to sent for approval please uncheck "Draft".</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
