    <validationRules>
        <fullName>ApprovedSRCannotBeChanged</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
QC_Approved__c == true, 
NOT(ISCHANGED(QC_Approved__c)), 
$Profile.Name != &apos;System Administrator&apos;, 
$Profile.Name != &apos;BMBS Sinotrust User&apos;, 
$Profile.Name != &apos;CAC IT Manager&apos;, 
OR(ISCHANGED(CustomerName__c), ISCHANGED(OriginalCustomerName__c), 
ISCHANGED(CustomerTitle__c),ISCHANGED(OfficePhone__c), 
ISCHANGED(OriginalOfficePhone__c), 
ISCHANGED(HomeMobilePhone__c), 
ISCHANGED(Original_HomeMobilePhone__c), 
ISCHANGED(CompanyName__c)) 
)</errorConditionFormula>
        <errorMessage>QC has been approved. You cannot modify SSI anymore.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Comments_Mandatory_SuspectCarModel</fullName>
        <active>true</active>
        <description>When field SuspectCarModel Review is &apos;Accept&apos;, Field &apos;Comments&apos; can not be blank</description>
        <errorConditionFormula>!ISCHANGED( Newly_Task_Id__c ) &amp;&amp;  
AND(  
ISCHANGED(CarModelCheck_Status__c ), 
ISPICKVAL(CarModelCheck_Status__c ,&apos;Accept&apos;) , 
ISBLANK(Comments__c )
 )</errorConditionFormula>
        <errorDisplayField>Comments__c</errorDisplayField>
        <errorMessage>SuspectCarModel Review is &apos;Accept&apos;, Field &apos;Comments&apos; can not be blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Comments_Mandatory_SuspectDealer_Review</fullName>
        <active>true</active>
        <description>when field SuspectDealer Review is &apos;Accept&apos;, &apos;Comments&apos; fields can not be blank.</description>
        <errorConditionFormula>!ISCHANGED( Newly_Task_Id__c ) &amp;&amp; 
AND(  
ISCHANGED(DealerCheck_Status__c ), 
ISPICKVAL(DealerCheck_Status__c ,&apos;Accept&apos;) , 
ISBLANK(Comments__c )
 )</errorConditionFormula>
        <errorDisplayField>Comments__c</errorDisplayField>
        <errorMessage>&apos;Comments&apos; fields can not be blank when field SuspectDealer Review is &apos;Accept&apos;</errorMessage>
    </validationRules>
