    <listViews>
        <fullName>All_Valid_SSI_Record</fullName>
        <columns>NAME</columns>
        <columns>Status__c</columns>
        <columns>InvoiceDate__c</columns>
        <columns>CustomerName__c</columns>
        <columns>HomeMobilePhone__c</columns>
        <columns>OfficePhone__c</columns>
        <columns>Dealer__c</columns>
