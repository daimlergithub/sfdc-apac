    <validationRules>
        <fullName>Add_Address_NoOfDigits</fullName>
        <active>true</active>
        <description>Additional Address should be less than or equal to 50 letters.</description>
        <errorConditionFormula>AND(LEN(Additional_Address__c)&gt;50, 
$Profile.Name != 'IntegrationAPI', 
OR(MD__c = 'JP',MD__c = 'KR',if(AND(MD__c=='MY',$Permission.MYGeneric),true,false)))</errorConditionFormula>
        <errorDisplayField>Additional_Address__c</errorDisplayField>
        <errorMessage>Additional Address should be less than or equal to 50 letters.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Address_NoOfDigits</fullName>
        <active>true</active>
        <errorConditionFormula>AND(LEN(Address__c)&gt;92, 
$Profile.Name != 'IntegrationAPI', 
OR(MD__c = 'JP',MD__c = 'KR'))</errorConditionFormula>
        <errorDisplayField>Address__c</errorDisplayField>
        <errorMessage>Address should be less than or equal to 92 letters.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Attention_NoOfDigits</fullName>
        <active>true</active>
        <errorConditionFormula>AND(LEN(Attention__c)&gt;50,
$Profile.Name != 'IntegrationAPI', 
MD__c = 'JP')</errorConditionFormula>
        <errorDisplayField>Attention__c</errorDisplayField>
        <errorMessage>Attention should be less than or equal to 50 letters.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DM_Request_Market_can_not_be_changed</fullName>
        <active>true</active>
        <description>Market field of DM Request should be same as user's market field</description>
        <errorConditionFormula>AND(TEXT( Market__c ) != TEXT( $User.Market__c ) ,$Profile.Name != 'IntegrationAPI', NOT(isTestRunning__c))</errorConditionFormula>
        <errorDisplayField>Market__c</errorDisplayField>
        <errorMessage>You are allowed to create record for your home market only</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Expected_DM_Material1</fullName>
        <active>true</active>
        <description>Expected DM Material1 has a value, but the Status 1 is null.
OR Status 1 is selected, but the Expected DM Material1 is null.</description>
        <errorConditionFormula>((Expected_DM_Material1__c != Null &amp;&amp;  ISPICKVAL(Status1__c, "")) || (Expected_DM_Material1__c = Null &amp;&amp; NOT(ISPICKVAL(Status1__c, "")))) &amp;&amp;  OR(MD__c = 'JP',MD__c = 'KR',MD__c = 'TH',if(AND(MD__c=='MY',$Permission.MYGeneric),true,false)) &amp;&amp; $Profile.Name != 'IntegrationAPI'</errorConditionFormula>
        <errorDisplayField>Expected_DM_Material1__c</errorDisplayField>
        <errorMessage>Please make sure both Expected DM Material and the status has a value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Expected_DM_Material2</fullName>
        <active>true</active>
        <description>Expected DM Material2 has a value, but the Catalogue Printing check 2  is null.
OR Catalogue Printing check 2  is selected, but the Expected DM Material2 is null.</description>
        <errorConditionFormula>((Expected_DM_Material2__c != Null &amp;&amp; ISPICKVAL(Status2__c, "")) || (Expected_DM_Material2__c = Null &amp;&amp; NOT(ISPICKVAL(Status2__c, "")))) &amp;&amp; OR(MD__c = 'JP',MD__c = 'KR',MD__c = 'TH',if(AND(MD__c=='MY',$Permission.MYGeneric),true,false)) &amp;&amp; $Profile.Name != 'IntegrationAPI'</errorConditionFormula>
        <errorDisplayField>Expected_DM_Material2__c</errorDisplayField>
        <errorMessage>Pleaes make sure both Expected DM Material and the status has a value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Expected_DM_Material3</fullName>
        <active>true</active>
        <description>Expected DM Material3 has a value, but the Catalogue Printing check 3  is null.
OR Catalogue Printing check 3  is selected, but the Expected DM Material3 is null.</description>
        <errorConditionFormula>((Expected_DM_Material3__c != Null &amp;&amp; ISPICKVAL(Status3__c, "")) || (Expected_DM_Material3__c = Null &amp;&amp; NOT(ISPICKVAL(Status3__c, "")))) &amp;&amp; OR(MD__c = 'JP',MD__c = 'KR',MD__c = 'TH',if(AND(MD__c=='MY',$Permission.MYGeneric),true,false)) &amp;&amp; $Profile.Name != 'IntegrationAPI'</errorConditionFormula>
        <errorDisplayField>Expected_DM_Material3__c</errorDisplayField>
        <errorMessage>Pleaes make sure both Expected DM Material and the status has a value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Insufficient_Stock</fullName>
        <active>true</active>
        <description>US-DM-003:
7. If Expected DM material's stock reaches Stock alert level, Data loader will report an error message ""Insufficient Stock"". And this record will not be uploaded successfully."</description>
        <errorConditionFormula>ISPICKVAL(Status__c , 'Sent') &amp;&amp; OR(MD__c = 'KR',MD__c = 'TH',if(AND(MD__c=='MY',$Permission.MYGeneric),true,false)) &amp;&amp; 
( 
Actual_DM_Material1__r.Threshold__c &gt; Actual_DM_Material1__r.Stock__c 
|| 
Actual_DM_Material2__r.Threshold__c &gt; Actual_DM_Material2__r.Stock__c 
|| 
Actual_DM_Material3__r.Threshold__c &gt; Actual_DM_Material3__r.Stock__c 
)</errorConditionFormula>
        <errorMessage>Insufficient Stock</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lead_is_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Delivery_Type__c,'4'),
OR(ISBLANK(Lead__c ), ISNULL(Lead__c )),
$Profile.Name != 'IntegrationAPI', 
MD__c = 'JP')</errorConditionFormula>
        <errorDisplayField>Lead__c</errorDisplayField>
        <errorMessage>Lead is required if your delivery type is 4.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Manual_Status_Change_Reject</fullName>
        <active>true</active>
        <description>You cannot manually select "Rejected".</description>
        <errorConditionFormula>AND(ISPICKVAL( Status__c , 'Rejected'),OR(MD__c = 'JP',MD__c = 'KR'),$Profile.Name != 'IntegrationAPI')</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>You cannot manually select "Rejected".</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Manual_Status_Change_To_Approve_Check</fullName>
        <active>true</active>
        <description>You cannot manually select "Approving".</description>
        <errorConditionFormula>AND(ISPICKVAL(Status__c,'Approving'),OR(MD__c = 'JP'),$Profile.Name != 'IntegrationAPI')</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>You cannot manually select "Approving".</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Manual_Status_Change_To_Be_Sent</fullName>
        <active>true</active>
        <description>You cannot manually select "to be sent".</description>
        <errorConditionFormula>AND(ISPICKVAL( Status__c , 'to be sent'),NOT(ISPICKVAL(PRIORVALUE(Status__c),'to be sent')),OR(MD__c = 'JP',MD__c = 'KR'),$Profile.Name != 'IntegrationAPI')</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>You cannot manually select "to be sent".</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Manual_Status_Change_To_Not_Send</fullName>
        <active>true</active>
        <description>You cannot manually select "Not send".</description>
        <errorConditionFormula>AND(ISPICKVAL( Status__c , 'Not send'),OR(MD__c = 'JP',MD__c = 'KR'),$Profile.Name != 'IntegrationAPI')</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>You cannot manually select "Not send".</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Negative_Stock_on_ExpectedDMMat1</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Expected_DM_Material1__c != Null,Expected_DM_Material1__r.Stock__c &lt;=0,$Profile.Name != 'IntegrationAPI',ISPICKVAL($Profile.UserType, 'Standard'), ISPICKVAL($User.Market__c,'KR'))</errorConditionFormula>
        <errorDisplayField>Expected_DM_Material1__c</errorDisplayField>
        <errorMessage>A selected DM Material does not have a sufficient stock</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Negative_Stock_on_ExpectedDMMat2</fullName>
