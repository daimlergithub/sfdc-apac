    <validationRules>
        <fullName>DM_Material_is_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Profile.Name !='IntegrationAPI',ISBLANK(DM_Material__c),ISPICKVAL(Execution_Type__c,"DM"))</errorConditionFormula>
        <errorDisplayField>DM_Material__c</errorDisplayField>
        <errorMessage>When Execution Type is DM, DM Material is Required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Expected_Outcome_Fields</fullName>
        <active>true</active>
        <errorConditionFormula>OR(Expected_Response_Rate__c &gt; 1,Expected_Participation__c &gt; 1,Expected_Registration__c &gt; 1, Expected_Lead__c &gt; 1)</errorConditionFormula>
        <errorMessage>Expected Outcome fields should not be more than 100.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Locked_for_Central_when_Published</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR(
 RecordType.Name == 'S&amp;M Event Central Campaign',
 RecordType.Name == 'S&amp;M Media Central Campaign',
 RecordType.Name == 'AS Central Campaign'
), 
 OR(PRIORVALUE(Status)== 'Published',PRIORVALUE(Status)=='Closed'),
 OR(  ISCHANGED( Campaign_Code__c ),
      ISCHANGED( Name ),
      ISCHANGED( Description ), 
      ISCHANGED( Brand__c ), 
      ISCHANGED( Plan_Start_Date__c ), 
      ISCHANGED( Plan_End_Date__c ), 
      ISCHANGED( Region__c ), 
      ISCHANGED( Actual_Region_Expense__c ), 
      ISCHANGED( Region_Budget__c ), 
	  ISCHANGED( Media_Type__c ), 
	  ISCHANGED( Event_Type__c ), 
	  ISCHANGED( CBU_PbP__c ), 
	  ISCHANGED( Other_Media_Type__c ), 
	  ISCHANGED( Other_Event_Type__c ), 
	  ISCHANGED( Car_Model__c ), 
	  ISCHANGED( Campaign_Type__c ), 
	  ISCHANGED( Driven__c ), 
	  ISCHANGED( Min_Car_Age__c ), 
	  ISCHANGED( Max_Car_Age__c ),
	  ISCHANGED( Purpose__c )
 ),
 $Profile.Name != 'IntegrationAPI'
)</errorConditionFormula>
        <errorMessage>You cann't change this record when Status is published or closed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Locked_for_MB_BP_when_Published</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
OR( 
RecordType.DeveloperName == 'MB_Best_Practice', 
RecordType.DeveloperName == 'smart_Best_Practice' 
), 
ISPICKVAL(Status, 'Published'), 
!ISCHANGED(Status), 
OR( ISCHANGED( Campaign_Code__c ), 
ISCHANGED( Name ), 
ISCHANGED( Status ), 
ISCHANGED( Description ), 
ISCHANGED( Brand__c ), 
ISCHANGED( BP_Start_Date__c ), 
ISCHANGED( BP_End_Date__c ), 
ISCHANGED( Region_Due_Date__c ), 
ISCHANGED( Retail_Start_Date__c ), 
ISCHANGED( Retail_Due_Date__c ), 
ISCHANGED( Region_Recommend_Number__c ), 
ISCHANGED( Region_Best_Number__c ), 
ISCHANGED( BP_Quarter__c ) 
), 
$Profile.Name != 'IntegrationAPI' 
)</errorConditionFormula>
        <errorMessage>You cann't change this record when Status is published.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MB_BP_fields_are_required_when_Publish</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
OR( 
AND( 
RecordType.DeveloperName == 'MB_Best_Practice', 
OR( 
ISNULL( BP_Start_Date__c ), 
ISNULL( BP_End_Date__c ), 
ISNULL( Region_Due_Date__c ), 
ISNULL( Retail_Start_Date__c ), 
ISNULL( Retail_Due_Date__c ), 
ISNULL( Region_Recommend_Number__c ), 
ISNULL( Region_Best_Number__c ) 
)), 
AND( 
RecordType.DeveloperName == 'smart_Best_Practice', 
OR( 
ISNULL( BP_Start_Date__c ), 
ISNULL( BP_End_Date__c ), 
ISNULL( Region_Due_Date__c ), 
ISNULL( Retail_Start_Date__c ), 
ISNULL( Retail_Due_Date__c ), 
ISNULL( Region_Best_Number__c ) 
)) 
), 
ISPICKVAL(Status, 'Published'), 
$Profile.Name != 'IntegrationAPI' 
)</errorConditionFormula>
        <errorMessage>“BP Start Date”, “BP End Date”, “Retail Start Date”, “Retail Due Date”, “Region Due Date”, “Region Best Number” and “Region Recommend Number” are required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Only_Started_Campaign_can_be_activated</fullName>
        <active>true</active>
        <description>When Status = Planned, Confirmed, Approved, Closed, Cancelled, Active can not be true.
Record Type = CAC CRM, Central Campaign, CAS Campaign</description>
        <errorConditionFormula>NOT(ISPICKVAL( Status , 'Started')) 
&amp;&amp; IsActive
&amp;&amp; (  RecordType.Name = "CAC CRM Campaign" || RecordType.Name = "Central Marketing Campaign" || RecordType.Name = "CAS Marketing Campaign")</errorConditionFormula>
        <errorDisplayField>IsActive</errorDisplayField>
        <errorMessage>Only Campaign with "Started" status can be activated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Previous_Campaign_Execution_Parent</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
Previous_Campaign_Execution__r.ParentId  &lt;&gt;  ParentId,
NOT(ISBLANK(Previous_Campaign_Execution__c))
)</errorConditionFormula>
        <errorDisplayField>Previous_Campaign_Execution__c</errorDisplayField>
        <errorMessage>Previous campaign execution only belong to this parent campaign.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SMS_Task_Verified_is_True</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
SMS_Task_Verified__c  = True,
NOT(ISPICKVAL(Execution_Type__c,"SMS")),NOT(ISPICKVAL(Execution_Type__c,"MMS"))
)</errorConditionFormula>
        <errorDisplayField>SMS_Task_Verified__c</errorDisplayField>
        <errorMessage>SMS task verified can only be true when Execution Type is SMS</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SMS_eDM_Template_is_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISBLANK(Template__c), 
OR( 
ISPICKVAL(Execution_Type__c ,"SMS"), 
ISPICKVAL(Execution_Type__c ,"MMS"), 
ISPICKVAL(Execution_Type__c ,"eDM") 
) 
)</errorConditionFormula>
        <errorDisplayField>Template__c</errorDisplayField>
        <errorMessage>When Execution Type is SMS/MMS/eDM, SMS/MMS/eDM Template is Required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>not_check_active_when_new_cac_camapgin</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
RecordType.Id = '01290000000rXlr',
ISNEW(),
IsActive = True
)</errorConditionFormula>
        <errorDisplayField>IsActive</errorDisplayField>
        <errorMessage>When you create new CAC camapgin, you can't check Active.</errorMessage>
    </validationRules>
