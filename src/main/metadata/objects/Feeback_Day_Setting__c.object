<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Configure the final feedback updating day for the retail compaign(record object), used in the button Unlock_Application_Feedback.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Actual_End_Day__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>The format is "3.31"</inlineHelpText>
        <label>Actual End Day</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Actual_Start_Day__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>The format is "1.1"</inlineHelpText>
        <label>Actual  Start Day</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Final_Feedback_Day__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>If you want to make the feedback day to the next year, please make sure the text looks like "Next Year 1.31" otherwise the system cannot parse the data correctly.</inlineHelpText>
        <label>Final Feedback Day</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Feeback Day Setting</label>
    <visibility>Protected</visibility>
</CustomObject>
