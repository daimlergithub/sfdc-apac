        <fullName>R_Generate_Leads_Volume__c</fullName>
        <externalId>false</externalId>
        <formula>IF(OR(ISPICKVAL(Lead_DataSource__c,'IB Call'),ISPICKVAL(Lead_DataSource__c,'OB Call')),1,0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Generate Leads Volume</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>R_SuccessfulCall__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISBLANK( Contact__r.Last_Successful_Call__c )==true,0,1)</formula>
        <label>SuccessfulCall</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>R_Task_Genereated_Amount__c</fullName>
        <externalId>false</externalId>
        <formula>IF(Task_Generated__c==true,1,0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Task Genereated Amount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>R_isCampaingLead__c</fullName>
        <externalId>false</externalId>
        <formula>IF(Name&lt;&gt;'',1,0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>isCampaingLead</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>RegistrationDate__c</fullName>
        <defaultValue>today()</defaultValue>
        <externalId>false</externalId>
        <label>Registration Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Successful_Call_Number__c</fullName>
        <externalId>false</externalId>
        <label>Successful Call Number</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Task_Generated__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Mark whether has generated the task for the campaign lead.</description>
        <externalId>false</externalId>
        <label>Task Generated</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Updated_By_EP_Transactions__c</fullName>
