        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Campaign Lead</label>
    <listViews>
        <fullName>All</fullName>
        <columns>NAME</columns>
        <columns>Campaign__c</columns>
        <columns>Lead__c</columns>
        <columns>Task_Generated__c</columns>
        <columns>CREATEDBY_USER</columns>
        <filterScope>Everything</filterScope>
        <label>All</label>
        <language>en_US</language>
    </listViews>
    <nameField>
        <displayFormat>CL-{000000}</displayFormat>
        <label>Campaign Lead No.</label>
        <trackHistory>false</trackHistory>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>Campaign Leads</pluralLabel>
    <searchLayouts/>
    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>Can_Not_Change_Registration_Date</fullName>
        <active>true</active>
        <description>You can NOT change the registration date of lead after enter first time.</description>
        <errorConditionFormula>ISCHANGED(RegistrationDate__c) &amp;&amp; PRIORVALUE(RegistrationDate__c) != null</errorConditionFormula>
        <errorDisplayField>RegistrationDate__c</errorDisplayField>
        <errorMessage>You can not change Registration Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Do_Not_Select_CAC_Campaign</fullName>
        <active>true</active>
        <errorConditionFormula>Campaign__r.RecordType.Id = '01290000000rXlr'</errorConditionFormula>
        <errorDisplayField>Campaign__c</errorDisplayField>
        <errorMessage>This Field can not be selected CAC Campaign Level.</errorMessage>
    </validationRules>
