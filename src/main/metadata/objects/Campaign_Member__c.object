    <validationRules>
        <fullName>StatusChangingFromExecuted</fullName>
        <active>true</active>
        <description>Camapaign Member Status can be only updated from Executed to Responded, Bounced or No Response</description>
        <errorConditionFormula>AND( 
ISPICKVAL(PRIORVALUE(Status__c) , "Executed"),MD__c = 'JP', $Profile.Name != 'IntegrationAPI', 
OR( 
ISPICKVAL( Status__c , "Removed"), 
ISPICKVAL( Status__c , "Ready") 
) 
)</errorConditionFormula>
        <errorMessage>Status cannot be changed from 'Executed' to 'Ready or Removed'</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>campaign_locked</fullName>
        <active>true</active>
        <errorConditionFormula>AND( MD__c = 'JP', $Profile.Name != 'IntegrationAPI', 
ISPICKVAL( PRIORVALUE( Status__c ) , "Responded"), 
OR( 
ISPICKVAL( Status__c , "Removed") , 
ISPICKVAL( Status__c , "Ready") , 
ISPICKVAL( Status__c , "Bounced") , 
ISPICKVAL( Status__c , "Executed") , 
ISPICKVAL( Status__c , "Not Responded") 

) 
)</errorConditionFormula>
        <errorMessage>Status Cannot be Changed When Status is Responded</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>retail_campaign_is_locked</fullName>
        <active>true</active>
        <errorConditionFormula>AND( OR( NOT(ispickval(Retail_Campaign_Id__r.Child_Campaign_Status__c ,'Segmentation')),NOT(ispickval(Retail_Campaign_Id__r.Child_Campaign_Status__c ,'Execution')))
,OR(ispickval(Retail_Campaign_Id__r.MBTH_Approval_Status__c,'Submitted'),ispickval(Retail_Campaign_Id__r.MBTH_Approval_Status__c,'Rejected')),MD__c='TH')</errorConditionFormula>
        <errorMessage>You Can Add Campaign Members in Segmentation or Execution and if it is sent for approval then it should get approval</errorMessage>
    </validationRules>
