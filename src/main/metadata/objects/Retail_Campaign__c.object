// zhongyang.si add for Release 1.3 CR6, check whether the user can update the retail campaign feedback,as the block day rule is changed
var unlockFeedbackCheck = false;
var planEndDate = "{!Retail_Campaign__c.Actual_End_Date__c}";
planEndDate = new Date(Date.parse(planEndDate.replace(/-/g,"/")));
var feedbackSetting = sforce.connection.query("SELECT Actual_Start_Day__c, Actual_End_Day__c, Final_Feedback_Day__c FROM Feeback_Day_Setting__c");
var it = new sforce.QueryResultIterator(feedbackSetting);
while (it.hasNext() &amp;&amp; !unlockFeedbackCheck) {
	var feedback = it.next();
	var today = new Date();
	var beginDate = new Date(planEndDate.getFullYear(),feedback.Actual_Start_Day__c.split(".")[0]-1,feedback.Actual_Start_Day__c.split(".")[1],0,0,0);
	var endDate = new Date(planEndDate.getFullYear(),feedback.Actual_End_Day__c.split(".")[0]-1, feedback.Actual_End_Day__c.split(".")[1],0,0,0);
	var finalDate;
	if ( feedback.Final_Feedback_Day__c.indexOf("Next Year") &gt;= 0 ){
		finalDate = new Date(planEndDate.getFullYear()+1,feedback.Final_Feedback_Day__c.replace(/Next Year/,"").split(".")[0]-1,feedback.Final_Feedback_Day__c.replace(/Next Year/,"").split(".")[1],0,0,0);
	} else {
		finalDate = new Date(planEndDate.getFullYear(),feedback.Final_Feedback_Day__c.split(".")[0]-1,feedback.Final_Feedback_Day__c.split(".")[1],0,0,0);
	}
	if ( planEndDate &gt;= beginDate &amp;&amp; planEndDate &lt;= endDate ){
		if ( today &gt; finalDate ){
			unlockFeedbackCheck = true;
		}
	}
}
