while(ait.hasNext()) { 
var RetailCampaign = ait.next(); 
AS_MarketingPlannedBudget = AS_MarketingPlannedBudget + parseFloat(RetailCampaign.Plan_Expense__c); 
PartsTurnover = PartsTurnover + parseFloat(RetailCampaign.Parts_Turn_over__c); 
LaborTurnover = LaborTurnover + parseFloat(RetailCampaign.Labor_Turn_over__c); 
InsuranceSold = InsuranceSold + parseFloat(RetailCampaign.Plan_No_of_Insurance_Sold__c); 
VehicleThroughput = VehicleThroughput + parseFloat(RetailCampaign.Order__c); 
