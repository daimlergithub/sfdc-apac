if(PartsTurnover &gt; 0){
callList.Y_Parts_Turnover__c = PartsTurnover; 
}
if(LaborTurnover &gt; 0){
callList.Y_Labor_Turnover__c = LaborTurnover; 
}
if(InsuranceSold  &gt; 0){
callList.Y_No_of_Insurance_Sold__c = InsuranceSold; 
}
if(VehicleThroughput &gt; 0){
callList.Y_Vehicle_Throughput__c = VehicleThroughput; 
}
if(MaintenanceProductsSold  &gt; 0){
callList.Y_No_of_Maintenance_Products_Sold__c = MaintenanceProductsSold; 
}
var yearlyUpdate = new Array(); 
yearlyUpdate.push(callList); 
try { 
var result = sforce.connection.update(yearlyUpdate); 
} catch(error) { 
alert("Failed to update Field with error: " + error); 
} 
location.href = location.href; 
} 
else{
alert('状态错误。'); 
}</url>
