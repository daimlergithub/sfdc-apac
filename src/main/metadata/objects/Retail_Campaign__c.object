        <url>{!REQUIRESCRIPT(&quot;/support/console/27.0/integration.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/27.0/apex.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/27.0/connection.js&quot;)} 
var qr3 = sforce.connection.query(&quot;select AccountId  from User where id = &apos;&quot;+&apos;{!$User.Id}&apos; +&quot;&apos;&quot;) ; 
if(&apos;{!Campaign.Status}&apos; == &apos;Published&apos;){
if(qr3.records.AccountId != null){
var qr1 = sforce.connection.query(&quot;Select name From recordtype where Id = &apos;&quot;+&apos;{!Campaign.RecordTypeId}&apos; +&quot;&apos;&quot;) ; 
var campaignRecordTypeName = qr1.records.Name;
var retailCampaignRecordTypeName;
switch (campaignRecordTypeName )
{
case &apos;AS BMBS Campaign&apos;:
  retailCampaignRecordTypeName =&quot;AS Retail Campaign&quot;;
  break;
case &apos;S&amp;M Event BMBS Campaign&apos;:
  retailCampaignRecordTypeName =&quot;S&amp;M Event Retail Campaign&quot;;
  break;
case &apos;S&amp;M Media BMBS Campaign&apos;:
  retailCampaignRecordTypeName =&quot;S&amp;M Media Retail Campaign&quot;;
  break;
}
var qr2 = sforce.connection.query(&quot;Select id From recordtype where Name= &apos;&quot;+ retailCampaignRecordTypeName +&quot;&apos; limit 1&quot;) ; 
var qr4 = sforce.connection.query(&quot;select Name from Account where id = &apos;&quot;+ qr3.records.AccountId+&quot;&apos;&quot;) ; 

var FromPickListField = &quot;{!Campaign.Car_Model__c}&quot;; 
var ToPickListFieldName =&quot;00N9000000BhOxf&quot;; 
var pairs = FromPickListField.split(&quot;;&quot;); 

var Fields = &quot;&quot; 
for (var i=0;i&lt;pairs.length;i++) { 
var ThisValue = pairs[i] ;
Fields = Fields + ToPickListFieldName+ &quot;=&quot; + ThisValue + &quot;&amp;&quot;;
 } 

var navigateToUrl = &quot;/a1j/e?CF00N9000000BhOyY={!Campaign.Name}&amp;CF00N9000000BhOyY_lkid={!Campaign.Id}&amp;CF00N9000000BhOxi=&quot;+qr4.records.Name+&quot;&amp;CF00N9000000BhOxi_lkid=&quot;+qr3.records.AccountId+&quot;&amp;RecordType=&quot;+qr2.records.Id+&quot;&amp;ent=Retail_Campaign__c&amp;Name={!Campaign.Name}&amp;00N9000000BhOxZ={!Campaign.Brand__c}&quot;+&quot;&amp;&quot;+Fields+&quot;&amp;00N9000000BhOyS={!Campaign.Plan_Start_Date__c}&amp;00N9000000BhOyJ={!Campaign.Plan_End_Date__c}&quot;
switch (retailCampaignRecordTypeName)
{
case &apos;AS Retail Campaign&apos;:
  navigateToUrl = navigateToUrl+&quot;&amp;00N9000000BhOyF={!Campaign.Min_Car_Age__c}&amp;00N9000000BhOyC={!Campaign.Max_Car_Age__c}&amp;00N9000000BhOxe={!Campaign.Campaign_Type__c}&quot;;
  break;
case &apos;S&amp;M Event Retail Campaign&apos;:
  navigateToUrl = navigateToUrl + &quot;&amp;00N9000000BhOxm=Joint&amp;00N9000000BhOxn={!Campaign.Event_Type__c}&amp;00N9000000BhOxa={!Campaign.CBU_PbP__c}&quot;;
  break;
case &apos;S&amp;M Media Retail Campaign&apos;:
  navigateToUrl = navigateToUrl + &quot;&amp;00N9000000BhOxm=Joint&amp;00N9000000BhOyE={!Campaign.Media_Type__c}&amp;00N9000000BhOxa={!Campaign.CBU_PbP__c}&quot;;
  break;
}
window.parent.navigateToUrl(navigateToUrl);
}else{
alert(&apos;权限错误&apos;);
}
}else{
