    <validationRules>
        <fullName>A_Start_Date_is_less_than_the_A_End_Date</fullName>
        <active>true</active>
        <description>Actual Start Date later than Actual Start Date</description>
        <errorConditionFormula>AND( 
OR(RecordType.Name =='AS Retail Campaign',
RecordType.Name =='S&amp;M Event Retail Campaign',
RecordType.Name =='S&amp;M Media Retail Campaign'
),
Actual_Start_Date__c &gt; Actual_End_Date__c,
$Profile.Name != 'IntegrationAPI'
)</errorConditionFormula>
        <errorDisplayField>Actual_End_Date__c</errorDisplayField>
        <errorMessage>Actual Start Date must be less than or equal to Actual End Dat</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Apply_Deadline_must_later_than_today</fullName>
        <active>true</active>
        <errorConditionFormula>IF( 
AND(
RecordType.Name == 'Sales &amp; Marketing Yearly TA', ISCHANGED(Y_Apply_Deadline__c),
$Profile.Name != 'IntegrationAPI'), 
Y_Apply_Deadline__c &lt; TODAY(), 
false)</errorConditionFormula>
        <errorDisplayField>Y_Apply_Deadline__c</errorDisplayField>
        <errorMessage>Apply Deadline must later than today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Auto_Segmentation_Criteria</fullName>
        <active>true</active>
        <description>If Auto Segmentation Check box is checked then Segmentation Criteria is Required</description>
        <errorConditionFormula>AND(($Profile.Name != 'IntegrationAPI'),( Auto_Segmentation__c = TRUE),OR(MD__c = 'JP',MD__c = 'TH', if(AND(MD__c=='MY',$Permission.MYDealer),true,false)),(ISBLANK(Segmentation_Criteria__c ) ))</errorConditionFormula>
        <errorMessage>If Auto Segmentation Check box is checked then Segmentation Criteria is Required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cant_add_members_once_segmentation_reach</fullName>
        <active>true</active>
        <description>You can't add Campaign member once the segmentation date is reached</description>
        <errorConditionFormula>AND(ISCHANGED( Total_Retail_Campaign_Members__c ),OR( RecordType.Name = 'Planning &amp; Design Campaign',RecordType.Name = 'Campaign Execution'),$Profile.Name != 'IntegrationAPI',(Segmentation_Date__c&gt;today()),if(AND(MD__c=='MY',$Permission.MYDealer),true,false))</errorConditionFormula>
        <errorMessage>You can't add Campaign member once the segmentation date is reached</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Couldnot_Update_Monthly</fullName>
        <active>true</active>
        <errorConditionFormula>AND(  
OR(ISPICKVAL(M_YearlyTA__r.Y_Status__c, 'Confirmed') , ISPICKVAL(M_YearlyTA__r.Y_Status__c, 'Approved') 
), 
RecordType.DeveloperName == 'Monthly_TA',
$Profile.Name != 'IntegrationAPI'
)</errorConditionFormula>
        <errorMessage>You can't update Monthly information with current YearlyTA status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Couldnot_Update_Region_Recommend</fullName>
        <active>true</active>
        <description>Couldnot_Update_Region_Recommend Region best practice is false.</description>
        <errorConditionFormula>AND(
OR( 
RecordType.Name == 'Retail MB Best Practice', 
RecordType.Name == 'Retail smart Best Practice' 
), 
 NOT(Region_Best_Practice__c),
 Region_Recommendation__c,
 $Profile.Name != 'IntegrationAPI'
)</errorConditionFormula>
        <errorMessage>Only the candidate is marked as Region Best, the Region Recommend can be selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Couldnot_Update_Retail_Candidate</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
OR( 
RecordType.Name == 'Retail MB Best Practice', 
RecordType.Name == 'Retail smart Best Practice' 
), 
or(
ISPICKVAL(BP_Status__c, 'Retail Submit'),
ISPICKVAL(BP_Status__c, 'Region Submit'),
ISPICKVAL(BP_Status__c, 'Final Submit')
),
ISCHANGED( Retail_Candidate__c),
$Profile.Name != 'IntegrationAPI'
)</errorConditionFormula>
        <errorMessage>You Couldn't Update Retail Candidate With Current Status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DM_Material_Template_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND
(
$Profile.Name !='IntegrationAPI',
ISBLANK(DM_Template__c),
ISPICKVAL(Execution_Type__c,"DM"),
Record_Type_Name__c = 'Campaign Execution',
OR(MD__c ='JP',MD__c ='TH')
)</errorConditionFormula>
        <errorDisplayField>DM_Template__c</errorDisplayField>
        <errorMessage>DM Material Template is reqired when execution type is DM</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Dealer_Couldnot_update_Retail_BP</fullName>
        <active>true</active>
        <description>Dealer Couldn't update Retail MB or smart BP After Retail Due Date - Generation Process</description>
        <errorConditionFormula>AND(
OR(
RecordType.Name == 'Retail MB Best Practice',
RecordType.Name == 'Retail smart Best Practice'
),
Parent_Campaign__r.Retail_Due_Date__c &gt;= TODAY() + 1,
ISCHANGED(Name),
ISCHANGED(Parent_Campaign__c),
ISCHANGED(Dealer_Name__c),
ISCHANGED(Brand__c),
ISCHANGED(BP_Status__c),
ISCHANGED(Comments__c),
ISCHANGED(Retail_Candidate__c),
ISCHANGED(BP_Category__c),
ISCHANGED(Region_Best_Practice__c),
ISCHANGED(Region_Recommendation__c),
ISCHANGED(Final_Best_Practice__c),
ISCHANGED(Final_Best_Practice_Forsmart__c),
$Profile.Name != 'System Administrator',
$Profile.Name != 'IntegrationAPI'
)</errorConditionFormula>
        <errorMessage>You couldn't update this record, because due date is reached.</errorMessage>
