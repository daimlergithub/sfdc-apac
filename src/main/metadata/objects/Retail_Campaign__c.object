var RTname = sforce.connection.query("Select name From RecordType Where id = " + "'" + qr.records.RecordTypeId + "'"); 

if(RTname.records.Name =='Sales &amp; Marketing Yearly TA'){ 

if(parseInt('{!Retail_Campaign__c.Y_SMART_Marketing_Budget__c}'.substring(1).replace(/,/g,'')) &lt;parseInt('{!Retail_Campaign__c.Y_SMART_Marketing_Expense__c}'.substring(1).replace(/,/g,'')) ){ 
errorMessage = errorMessage +'SMART must be greater than or equal to the total marketing budget SMART market overall objectives.\n' 
} 

if(parseInt('{!Retail_Campaign__c.Y_AMG_Marketing_Budget__c}'.substring(1).replace(/,/g,'')) &lt;parseInt('{!Retail_Campaign__c.Y_AMG_Marketing_Expense__c}'.substring(1).replace(/,/g,''))){ 
errorMessage = errorMessage +'AMG Total marketing budget overall objectives must be greater than or equal to the AMG market.\n' 
} 
if(parseInt('{!Retail_Campaign__c.Y_MB_Marketing_Budget__c}'.substring(1).replace(/,/g,'')) &lt;parseInt('{!Retail_Campaign__c.Y_MB_Marketing_Expense__c}'.substring(1).replace(/,/g,''))){ 
errorMessage = errorMessage +'MB must be greater than or equal to the total marketing budget MB total goals. ' 
} 
if(parseInt('{!Retail_Campaign__c.Y_MB_Joint_Marketing_Budget__c}'.substring(1).replace(/,/g,'')) &lt;parseInt('{!Retail_Campaign__c.Y_MB_Joint_Marketing_Expense__c}'.substring(1).replace(/,/g,''))){ 
errorMessage = errorMessage +'Joint marketing activities must be greater than or equal to the joint activity targets. ' 
} 
if(parseInt('{!Retail_Campaign__c.Y_MB_Dealer_Driven_Marketing_Budget__c}'.substring(1).replace(/,/g,'')) &lt;parseInt('{!Retail_Campaign__c.Y_MB_Dealer_Driven_Marketing_Expense__c}'.substring(1).replace(/,/g,''))){ 
errorMessage = errorMessage +'Self-organizing activity budgets must be greater than or equal to organise activities targeting. ' 
} 
}else if(RTname.records.Name =='Aftersales Yearly TA'){ 
	if(parseInt('{!Retail_Campaign__c.Y_After_sales_Marketing_Planned_Budget__c}'.substring(1).replace(/,/g,'')) &lt; parseInt('{!Retail_Campaign__c.Y_After_sales_Marketing_Target_Budget__c}'.substring(1).replace(/,/g,'')) ){ 
	errorMessage = errorMessage +'Aftermarket budget overall objectives must be greater than or equal to the aftermarket.\n' 
	} 
}
if(errorMessage != '') 
{ 
alert(errorMessage); 
}else if ((Modal.confirm &amp;&amp; Modal.confirm('If you submit this application for recording, you will not be able to modify or cancel the application. Do you want to continue?')) || (!Modal.confirm &amp;&amp; window.confirm('If you submit this application for recording, you will not be able to modify or cancel the application. Do you want to continue?'))) { 
var callList = new sforce.SObject("Retail_Campaign__c"); 
callList.Id = '{!Retail_Campaign__c.Id}'; 
callList.Y_Status__c = "Confirmed"; 
var campaignUpdate = new Array(); 
campaignUpdate.push(callList); 
var callCompleted = false; 
try { 
var result = sforce.connection.update(campaignUpdate); 

if(result[0].success=='true'){ 
callCompleted = true; 
}else{ 
    var msg = "";
    if (typeof result[0].errors.length === 'undefined') {
        msg = result[0].errors.message;
    } else {
        for (var i=0;i&lt;result[0].errors.length;i++){
            msg = msg + result[0].errors[i].message + '\n';
        }
        msg = msg.substr(0, (msg.length - 1));
    }
    alert(msg);
    callCompleted = false;
} 
}catch(error) { 
alert("Failed to update Field with error: " + error); 
} 
if(callCompleted){ 
navigateToUrl('/p/process/Submit?id={!Retail_Campaign__c.Id}&amp;retURL=%2F{!Retail_Campaign__c.Id}');} 
} 
}else{ 
alert('Application status error.'); 
