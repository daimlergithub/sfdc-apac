var unlockFeedbackCheck = false;
var planEndDate = &quot;{!Retail_Campaign__c.Actual_End_Date__c}&quot;;
planEndDate = new Date(Date.parse(planEndDate.replace(/-/g,&quot;/&quot;)));
var feedbackSetting = sforce.connection.query(&quot;SELECT Actual_Start_Day__c, Actual_End_Day__c, Final_Feedback_Day__c FROM Feeback_Day_Setting__c&quot;);
var it = new sforce.QueryResultIterator(feedbackSetting);
while (it.hasNext() &amp;&amp; !unlockFeedbackCheck) {
	var feedback = it.next();
	var today = new Date();
	var beginDate = new Date(planEndDate.getFullYear(),feedback.Actual_Start_Day__c.split(&quot;.&quot;)[0]-1,feedback.Actual_Start_Day__c.split(&quot;.&quot;)[1],0,0,0);
	var endDate = new Date(planEndDate.getFullYear(),feedback.Actual_End_Day__c.split(&quot;.&quot;)[0]-1, feedback.Actual_End_Day__c.split(&quot;.&quot;)[1],0,0,0);
	var finalDate;
	if ( feedback.Final_Feedback_Day__c.indexOf(&quot;Next Year&quot;) &gt;= 0 ){
		finalDate = new Date(planEndDate.getFullYear()+1,feedback.Final_Feedback_Day__c.replace(/Next Year/,&quot;&quot;).split(&quot;.&quot;)[0]-1,feedback.Final_Feedback_Day__c.replace(/Next Year/,&quot;&quot;).split(&quot;.&quot;)[1],0,0,0);
	} else {
		finalDate = new Date(planEndDate.getFullYear(),feedback.Final_Feedback_Day__c.split(&quot;.&quot;)[0]-1,feedback.Final_Feedback_Day__c.split(&quot;.&quot;)[1],0,0,0);
	}
	if ( planEndDate &gt;= beginDate &amp;&amp; planEndDate &lt;= endDate ){
		if ( today &gt; finalDate ){
			unlockFeedbackCheck = true;
		}
	}
}
