    <validationRules>
        <fullName>AccountLink_Market_Cannot_be_Changed</fullName>
        <active>true</active>
        <description>Market field of AccountLink should be same as user's market field</description>
        <errorConditionFormula>AND(TEXT( Market__c ) != TEXT( $User.Market__c ) ,$Profile.Name != 'IntegrationAPI',NOT(istestRunning__c))</errorConditionFormula>
        <errorDisplayField>Market__c</errorDisplayField>
        <errorMessage>You are allowed to create record for your home market only</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Account_1_Account_2_cannot_be_same</fullName>
        <active>true</active>
        <description>Cannot link same accounts</description>
        <errorConditionFormula>(fromRole__c = toRole__c) &amp;&amp;
$Profile.Name != 'IntegrationAPI' &amp;&amp;
RecordType.DeveloperName = 'Contact2Contact'</errorConditionFormula>
        <errorMessage>Cannot Link to the same person</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Dealer_User_Cant_Change_toRole_Fromrole</fullName>
        <active>true</active>
        <description>Dealer users cant change the from role and torole</description>
        <errorConditionFormula>AND(!ISNEW(),OR($Profile.Name ='Japan Dealer Company Manager',$Profile.Name ='Japan Dealer Sales Manager',$Profile.Name ='Japan Dealer Sales Representative',$Profile.Name ='Japan Dealer Service Advisor',$Profile.Name ='Japan Dealer Service Manager' ),MD__c='JP',OR(RecordType.DeveloperName='Retail_Person',RecordType.DeveloperName='Retail_Company'),OR(ISCHANGED(fromRole__c),ISCHANGED(toRole__c)))</errorConditionFormula>
        <errorMessage>Dealer User Cant Change the from Role and To role</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Name_field_cannot_be_changed</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISCHANGED(Name),OR($Profile.Name = 'Japan Dealer Company Manager', $Profile.Name = 'Japan Dealer Sales Manager', $Profile.Name = 'Japan Dealer Sales Representative', $Profile.Name = 'Japan Dealer Service Advisor', $Profile.Name = 'Japan Dealer Service Manager'),OR(RecordType.DeveloperName = 'IndividualExternalLink',RecordType.DeveloperName = 'Vehicle_External_Link'),MD__c = 'JP')</errorConditionFormula>
        <errorMessage>Name field cannot be changed</errorMessage>
    </validationRules>
