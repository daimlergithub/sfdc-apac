    <webLinks>
        <fullName>Delete_retail_copy</fullName>
        <availability>online</availability>
        <description>Created for SFDCJP-753</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Delete retail copy</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT("/soap/ajax/29.0/connection.js")} 
{!REQUIRESCRIPT("/soap/ajax/29.0/apex.js")} 
var RetailCopyflag = "{!Account_Link__c.Retail_Delete_Flag__c}"; 
var RTType = "{!Account_Link__c.RecordTypeId}"; 
var RTs = sforce.connection.query("Select Id, Name from RecordType where sObjectType = 'Account_Link__c' and developerName ='Retail_Company'"); 
var rt = RTs.getArray("records"); 
var RTs1 = sforce.connection.query("Select Id, Name from RecordType where sObjectType = 'Account_Link__c' and developerName ='Retail_Person'"); 
var rt1 = RTs1.getArray("records"); 
var market = "{!Account_Link__c.MD__c}"; 

if(market == 'JP'){ 
if(RetailCopyflag != true &amp;&amp; RTType != rt[0].Id &amp;&amp; RTType != rt1[0].Id){ 
var myWindow = window.open("{!URLFOR($Site.Prefix)}/apex/ALKDeleteConformationpage?Id={!Account_Link__c.Id}", "_blank","resizable=yes,top=250,left=500,width=400,height=250"); 
} 
else { 
alert("{!$Label.AlreadyProcessed}"); 
} 
}else if(market == 'KR'){ 
alert("{!$Label.DeleteretailcopyErrorMsg}"); 
}</url>
    </webLinks>
    <webLinks>
        <fullName>Send_To_AutoLine</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Send To AutoLine</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT("/soap/ajax/29.0/connection.js")}
{!REQUIRESCRIPT("/soap/ajax/29.0/apex.js")}
var r = confirm("{!$Label.Are_you_sure_want_to_send_information_to_Auto_Line_interface}");

if(r == true)
{ 
var resultMsg= 
sforce.apex.execute("UtilWebservice","AccountLinkIfnoSendToAL",{RTCustomerId:"{!Account_Link__c.Id__c}",DealerID:"{!Account_Link__c.fromRoleId__c}",CorrelationId:"{!Account_Link__c.CorrelationID__c}"});
if(resultMsg!=null)
{
var alrt = '{!JSENCODE($Label.Successfully_Sent_Information_to_AutoLine)}'; 
alert(alrt); 
function updateAccountLink(){
//Create a New Account and Set the Fields
var accountlinkToUpdate = new sforce.SObject("Account_Link__c");
accountlinkToUpdate.Id = "{!Account_Link__c.Id}";
accountlinkToUpdate.CorrelationID__c = " ";

//Save it to the Database
var updateResult = sforce.connection.update([accountlinkToUpdate]);
//If the Update was Successful Refresh the Page
if(updateResult[0].success === "true"){
location.reload();
}
//Else Show the Error
else{
alert(updateResult[0].errors.message);
}
}
var correlation = "{!Account_Link__c.CorrelationID__c}";

if(correlation != " " || correlation !=null){
updateAccountLink();

}

}
else
{
var alrt = '{!JSENCODE($Label.Web_service_callout_failed)}'; 
alert(alrt); 
}
}</url>
    </webLinks>
