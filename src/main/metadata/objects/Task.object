{!REQUIRESCRIPT("/support/console/27.0/integration.js")} 
var idList = {!GETRECORDIDS($ObjectType.Task)}; 
if (idList == null || idList.length == 0) {
alert('Please select record(s).'); 
}else {
var taskUpdate = new Array();
for (var i = 0; i &lt; idList.length; i++) {
var callList = new sforce.SObject("Task");
callList.Id = idList[i];
callList.Status= "Closed";
taskUpdate.push(callList);
}
var callCompleted = false;
try {
var result = sforce.connection.update(taskUpdate);
callCompleted = true;
} catch(error) {
alert("Failed to update Field with error: " + error);
}
if (callCompleted) {
for (var i = 0; i &lt; result.length; i++) {
if (!result[i].getBoolean("success")) {
alert("Job Application (id='" + idList[i] +
"') could not be updated with error: " +
result[i].errors);
}
}
window.location.reload(true);
}
}</url>
