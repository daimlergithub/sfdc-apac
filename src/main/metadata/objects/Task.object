    <validationRules>
        <fullName>Activity_Status_is_Required</fullName>
        <active>true</active>
        <description>Task is being closed then the "Activity Status" should be mandatorily entered.</description>
        <errorConditionFormula>OR(AND($Profile.Name != 'IntegrationAPI',	
RecordType.DeveloperName = 'SSI_Task', 
ISPICKVAL(Status, 'Closed'), 
ISPICKVAL( Activity_Status__c, '' ),OR(MD__c= 'JP',MD__c='KR')),AND($Profile.Name != 'IntegrationAPI',ISPICKVAL(Status,'Closed'),ISPICKVAL( Activity_Status__c, '' ),OR(MD__c='TH',if(AND(MD__c=='MY',$Permission.MYGeneric),true,false))))</errorConditionFormula>
        <errorDisplayField>Related_Task_Id__c</errorDisplayField>
        <errorMessage>Activity status is required if you close task.</errorMessage>
    </validationRules>
	<validationRules>
        <fullName>Cannot_Edit_OB_Call_Task_with_Cancel_AS</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(PRIORVALUE(Activity_Status__c),'Cancelled'), MD__c = 'KR', RecordType.Name = 'OB Call')</errorConditionFormula>
        <errorMessage>These record cannot be edited as Activity Status is Cancelled</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>QC_Fields_editing_limitations</fullName>
        <active>false</active>
        <description>During QC execution, QC user should only able to edit fields"Generate QC" &amp; "QC Quantity".</description>
        <errorConditionFormula>(( $Profile.Name = "CAC IB QC") || ($Profile.Name = "CAC OB QC"))&amp;&amp; NOT(ISCHANGED( Generate_QC__c )) &amp;&amp; NOT((ISCHANGED( QC_Quantity__c )))</errorConditionFormula>
        <errorMessage>QC User can only edit "QC Quantity" and "Generate QC" field on this record</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>THAILAND_Block_List_Activity_Status</fullName>
