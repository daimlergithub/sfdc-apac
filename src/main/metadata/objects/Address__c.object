    <validationRules>
        <fullName>AddressLine1_NoOfDigit</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Profile.Name != 'IntegrationAPI',OR(UPPER(MD__c) = 'JP',UPPER(MD__c) = 'KR') ,OR(RecordType.DeveloperName ='Company',RecordType.DeveloperName ='Individual'),IF( LEN(  Address_Line_1__c  ) &gt; 50 ,true,false) )</errorConditionFormula>
        <errorDisplayField>Address_Line_1__c</errorDisplayField>
        <errorMessage>Address Line 1 should be less than or equal to 50 letters.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AddressLine1_NoOfDigit_100Char</fullName>
