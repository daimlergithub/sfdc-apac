        <errorConditionFormula>AND(NOT(
OR(
ISBLANK(Province_Native__c),
REGEX(Province_Native__c, "[]*"))),$Profile.Name != 'IntegrationAPI', 
MD__c = 'JP')</errorConditionFormula>
        <errorDisplayField>Province_Native__c</errorDisplayField>
        <errorMessage>It should be "Kana" characters.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Primary_Address_Reference_Not_Editable</fullName>
