    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>Address_Type_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Profile.Name != 'IntegrationAPI',UPPER(MD__c) = 'JP', ISPICKVAL(  Address_Type__c  ,''))</errorConditionFormula>
        <errorDisplayField>Address_Type__c</errorDisplayField>
        <errorMessage>Address Type is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Company_Name_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(ISCHANGED(TitleOfHonor__c),ISNEW()),
ISPICKVAL(TitleOfHonor__c,'Dear'),
ISBLANK(Company_Name__c), 
$Profile.Name != 'IntegrationAPI',
MD__c = 'JP'   
)</errorConditionFormula>
        <errorDisplayField>Company_Name__c</errorDisplayField>
        <errorMessage>Company name is required when TitleOfHonor is 'Dear'.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Company_Name_Single</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(REGEX(Company_Name_Native__c,"[^\\x00-\\x7F]+")),
NOT(ISBLANK(Company_Name_Native__c)),
$Profile.Name != 'IntegrationAPI', 
MD__c = 'JP')</errorConditionFormula>
        <errorDisplayField>Company_Name_Native__c</errorDisplayField>
        <errorMessage>Company name native should be in double byte.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invalid_ZipCode</fullName>
        <active>true</active>
        <description>Post Code is not 7 digits long</description>
        <errorConditionFormula>AND($Profile.Name != 'IntegrationAPI',
MD__c = 'JP', 
OR(ISCHANGED(ZipCode__c),ISNEW()), 
NOT(ISBLANK(ZipCode__c)),
NOT(REGEX(ZipCode__c,"[0-9]{7}")) 
)</errorConditionFormula>
        <errorDisplayField>ZipCode__c</errorDisplayField>
        <errorMessage>Please enter a 7 digit Zip code.</errorMessage>
    </validationRules>
</CustomObject>
