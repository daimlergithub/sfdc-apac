   OR( ISBLANK(Address_Line_1__c),ISNULL(Address_Line_1__c) )
)</errorConditionFormula>
        <errorDisplayField>Address_Line_1__c</errorDisplayField>
        <errorMessage>Address_Line_1__c is Mandatory</errorMessage>
    </validationRules>
