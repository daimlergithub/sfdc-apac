    <validationRules>
        <fullName>Foreign_National_unckeck_criteria</fullName>
        <active>true</active>
        <description>To stop user from selecting india country when foreign national field is selected</description>
        <errorConditionFormula>AND($Profile.Name != 'IntegrationAPI',$Permission.INGeneric,Foreign_Nationals__c == true,  ISPICKVAL(Country__c,'India'))</errorConditionFormula>
        <errorMessage>Cannot select Country as India when Foreign Nationals checkbox is ticked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IN_Invalid_City</fullName>
        <active>true</active>
        <description>City must not contain any special character and numbers</description>
        <errorConditionFormula>AND($Profile.Name != 'IntegrationAPI', 
$Permission.INGeneric, 
ISPICKVAL(Country__c , 'India'), 
OR(ISCHANGED( District__c ),ISNEW()), 
NOT(ISBLANK(District__c )), 
NOT(REGEX( District__c ,"^[a-z A-Z]*$")) 
)</errorConditionFormula>
        <errorMessage>City field must not contain any special Characters and Digits</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IN_Invalid_PostCode</fullName>
        <active>true</active>
        <description>Added for india market to validate postal code are 6 digits</description>
        <errorConditionFormula>AND($Profile.Name != 'IntegrationAPI', 
$Permission.INGeneric, 
ISPICKVAL(Country__c , 'India'), 
OR(ISCHANGED(ZipCode__c),ISNEW()), 
NOT(ISBLANK(ZipCode__c)), 
NOT(REGEX(ZipCode__c,"[0-9]{6}")) 
)</errorConditionFormula>
        <errorMessage>Postal code should be 6 digits long.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invalid_PostCode</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Profile.Name != 'IntegrationAPI',
$Permission.INGeneric, 
OR(ISCHANGED(ZipCode__c),ISNEW()),  TEXT( Country__c ) = 'India',
NOT(ISBLANK(ZipCode__c)),
NOT(REGEX(ZipCode__c,"[0-9]{6}")) 
)</errorConditionFormula>
        <errorDisplayField>ZipCode__c</errorDisplayField>
        <errorMessage>Post code should be 6 digits long.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invalid_ZipCode</fullName>
        <active>true</active>
        <description>Post Code is not 7 digits long</description>
        <errorConditionFormula>AND($Profile.Name != 'IntegrationAPI',
MD__c = 'JP', 
OR(ISCHANGED(ZipCode__c),ISNEW()), 
NOT(ISBLANK(ZipCode__c)),
NOT(REGEX(ZipCode__c,"[0-9]{7}")) 
)</errorConditionFormula>
        <errorDisplayField>ZipCode__c</errorDisplayField>
        <errorMessage>Please enter a 7 digit Zip code.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MBK_Invalid_ZipCode</fullName>
        <active>true</active>
        <description>This validation rule prevents users from entering invalid Zipcode/PostCode.</description>
        <errorConditionFormula>AND($Profile.Name != 'IntegrationAPI', 
MD__c = 'KR', 
OR(ISCHANGED(ZipCode__c)), 
NOT(ISBLANK(ZipCode__c)), 
NOT(REGEX(ZipCode__c,"[0-9]{5}")) 
)</errorConditionFormula>
        <errorDisplayField>ZipCode__c</errorDisplayField>
        <errorMessage>ZIP CODE should be minimum 5 digits and ONLY Numbers.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MBK_ZIP_CODE_is_mandatory</fullName>
        <active>true</active>
        <description>This validation rule ensures that Zipcode is entered mandatorily.</description>
        <errorConditionFormula>AND($Profile.Name != 'IntegrationAPI',
     OR(MD__c = 'JP',MD__c = 'KR'),
     OR(ISCHANGED(ZipCode__c)),
     OR( ISBLANK(ZipCode__c),ISNULL(ZipCode__c) )
    )</errorConditionFormula>
        <errorDisplayField>ZipCode__c</errorDisplayField>
        <errorMessage>ZIPCODE is mandatory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prefecture_Native_Kana</fullName>
