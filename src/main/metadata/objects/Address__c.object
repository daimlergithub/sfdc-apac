    <validationRules>
        <fullName>Address_Mandatory_IN</fullName>
        <active>true</active>
        <description>validation for user to enter address fields if Address Type is selected in case of INDIA market.</description>
        <errorConditionFormula>AND($Profile.Name !='IntegrationAPI',
!ISPICKVAL(Address_Type__c ,''),
$Permission.INGeneric,
OR(
Address_Line_1__c  = '',
District__c = '',
ISPICKVAL( Province__c ,''),
ISPICKVAL(Country__c , ''),
ZipCode__c = ''
))</errorConditionFormula>
        <errorMessage>For Address creation, Address Line 1, Country, State, City, Zip code are mandatory.</errorMessage>
    </validationRules>
