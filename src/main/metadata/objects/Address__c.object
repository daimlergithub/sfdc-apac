    <validationRules>
        <fullName>AddressLine1_NoOfDigit</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Profile.Name != 'IntegrationAPI',OR(UPPER(MD__c) = 'JP',UPPER(MD__c) = 'KR') ,OR(RecordType.DeveloperName ='Company',RecordType.DeveloperName ='Individual'),IF( LEN(  Address_Line_1__c  ) &gt; 50 ,true,false) )</errorConditionFormula>
        <errorDisplayField>Address_Line_1__c</errorDisplayField>
        <errorMessage>Address Line 1 should be less than or equal to 50 letters.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AddressLine1_NoOfDigit_TH</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Profile.Name != 'IntegrationAPI',UPPER(MD__c) = 'TH' ,IF( LEN(  Address_Line_1__c  ) &gt; 35 ,true,false) )</errorConditionFormula>
        <errorDisplayField>Address_Line_1__c</errorDisplayField>
        <errorMessage>Address Line 1 should be less than or equal to 35 characters.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AddressLine2_NoOfDigit</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Profile.Name != 'IntegrationAPI',OR(UPPER(MD__c) = 'JP',UPPER(MD__c) = 'KR'),OR(RecordType.DeveloperName ='Company',RecordType.DeveloperName ='Individual'),IF( LEN( Address_Line_2__c ) &gt; 50 ,true,false))</errorConditionFormula>
        <errorMessage>Address Line 2 should be less than or equal to 50 letters.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AddressLine2_NoOfDigit_TH</fullName>
        <active>true</active>
