    <validationRules>
        <fullName>End_Time_greater_than_Start_Time</fullName>
        <active>true</active>
        <description>End_Time_greater_than_Start_Time</description>
        <errorConditionFormula>End_Time__c &lt; Start_Time__c</errorConditionFormula>
        <errorDisplayField>End_Time__c</errorDisplayField>
        <errorMessage>End Time should be greater than Start Time.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Time_greater_than_today</fullName>
        <active>true</active>
        <description>End_Time_greater_than_today</description>
        <errorConditionFormula>End_Time__c &lt; DATETIMEVALUE(TODAY())</errorConditionFormula>
        <errorDisplayField>End_Time__c</errorDisplayField>
        <errorMessage>End Time should be greater than or equal to today's time.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Expired_Events_Rule</fullName>
        <active>true</active>
        <description>Do Not allow Users to EDIT Expired Events</description>
        <errorConditionFormula>Expired__c  = true</errorConditionFormula>
        <errorMessage>Event was already Expired.You are not allowed to EDIT Expired Event.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reg_End_Time_greater_than_Start_Time</fullName>
        <active>true</active>
        <description>Reg_End_Time_greater_than_Start_Time</description>
