<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <actionOverrides>
        <actionName>Accept</actionName>
        <type>Default</type>
    </actionOverrides>
    <actionOverrides>
        <actionName>CancelEdit</actionName>
        <type>Default</type>
    </actionOverrides>
    <actionOverrides>
        <actionName>Clone</actionName>
        <type>Default</type>
    </actionOverrides>
    <actionOverrides>
        <actionName>Delete</actionName>
        <type>Default</type>
    </actionOverrides>
    <actionOverrides>
        <actionName>Edit</actionName>
        <type>Default</type>
    </actionOverrides>
    <actionOverrides>
        <actionName>List</actionName>
        <type>Default</type>
    </actionOverrides>
    <actionOverrides>
        <actionName>New</actionName>
        <type>Default</type>
    </actionOverrides>
    <actionOverrides>
        <actionName>SaveEdit</actionName>
        <type>Default</type>
    </actionOverrides>
    <actionOverrides>
        <actionName>Tab</actionName>
        <type>Default</type>
    </actionOverrides>
    <actionOverrides>
        <actionName>View</actionName>
        <type>Default</type>
    </actionOverrides>
    <allowInChatterGroups>false</allowInChatterGroups>
    <compactLayoutAssignment>SYSTEM</compactLayoutAssignment>
    <deploymentStatus>Deployed</deploymentStatus>
    <description>Keep customer's reply here and to be displayed under SMS task</description>
    <enableActivities>false</enableActivities>
    <enableBulkApi>true</enableBulkApi>
    <enableChangeDataCapture>false</enableChangeDataCapture>
    <enableFeeds>false</enableFeeds>
    <enableHistory>false</enableHistory>
    <enableReports>true</enableReports>
    <enableSearch>true</enableSearch>
    <enableSharing>true</enableSharing>
    <enableStreamingApi>true</enableStreamingApi>
    <externalSharingModel>ReadWrite</externalSharingModel>
    <fields>
        <fullName>ContactName__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <label>Contact Name</label>
        <referenceTo>Contact</referenceTo>
        <relationshipLabel>SMS Reply Histories</relationshipLabel>
        <relationshipName>SMSReplyHistories</relationshipName>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>IsFirstValid__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <label>Is First Valid</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>IsValid__c</fullName>
        <defaultValue>true</defaultValue>
        <externalId>false</externalId>
        <label>Is Valid?</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>MMSTemplate__c</fullName>
        <externalId>false</externalId>
        <label>MMS Template</label>
        <length>20</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>RelatedCampaign__c</fullName>
        <externalId>false</externalId>
        <label>Related Campaign</label>
        <length>200</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>RespondTime__c</fullName>
        <description>Real time end user replies SMS</description>
        <externalId>false</externalId>
        <label>Respond Time</label>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>ResponseContent__c</fullName>
        <description>The whole message from end user and will be divided into SMS Content&amp;SMS Code by coding,must be hidden to all Salesforce users</description>
        <externalId>false</externalId>
        <label>Response Content</label>
        <length>200</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>SMSCode__c</fullName>
        <description>Customer reply code</description>
        <externalId>false</externalId>
        <label>SMS Code</label>
        <length>4</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>SMSContent__c</fullName>
        <description>SMS Reply Content</description>
        <externalId>false</externalId>
        <label>SMS Content</label>
        <length>1002</length>
        <trackTrending>false</trackTrending>
        <type>LongTextArea</type>
        <visibleLines>3</visibleLines>
    </fields>
    <fields>
        <fullName>SMSSentTime__c</fullName>
        <description>Real time SMS sent from Fractalist</description>
        <externalId>false</externalId>
        <label>SMS Sent Time</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>SMSTask__c</fullName>
        <description>To display task as a link</description>
        <externalId>false</externalId>
        <formula>HYPERLINK(TaskID__c, TaskID__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>SMS Task</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>SalesPerson__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <label>Sales Person</label>
        <referenceTo>User</referenceTo>
        <relationshipName>SMSReplyHistories</relationshipName>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>TaskID__c</fullName>
        <description>Display SMS Task only, this field will be hidden</description>
        <externalId>false</externalId>
        <label>Task ID</label>
        <length>20</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Template__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <label>Template</label>
        <referenceTo>Template__c</referenceTo>
        <relationshipLabel>SMS Reply Histories</relationshipLabel>
        <relationshipName>SMSReplyHistories</relationshipName>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>ValidReply__c</fullName>
        <description>Not shown to user, just for VF section display</description>
        <externalId>false</externalId>
        <formula>IF( IsValid__c , "Yes", "No")</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Valid Reply?</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>SMS Reply History</label>
    <listViews>
        <fullName>All</fullName>
        <columns>NAME</columns>
        <columns>ContactName__c</columns>
        <columns>IsValid__c</columns>
        <columns>ResponseContent__c</columns>
        <columns>RespondTime__c</columns>
        <columns>SMSSentTime__c</columns>
        <columns>Template__c</columns>
        <filterScope>Everything</filterScope>
        <label>All</label>
        <language>en_US</language>
    </listViews>
    <nameField>
        <displayFormat>SR-{0000}</displayFormat>
        <label>SMS Reply History Name</label>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>SMS Reply Histories</pluralLabel>
    <searchLayouts/>
    <sharingModel>ReadWrite</sharingModel>
    <visibility>Public</visibility>
</CustomObject>
