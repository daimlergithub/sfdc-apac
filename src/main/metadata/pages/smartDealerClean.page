<apex:page controller="smartDealerCleanController" tabStyle="smart_SSI__c">
    <!-- Remove the default Focus of the first inputField -->
    <script>function setFocusOnLoad() {}</script>
    <apex:form >
        <!-- 错误提示 -->
        <apex:pageMessages id="message" />
        <apex:sectionHeader title="SSI_经销商清洗" />
        <apex:pageBlock title="{!$Label.SSI_Invoice_Date_Page_Section_Title}">
            <apex:pageBlockSection columns="1">
            
            <apex:inputField value="{!fromSSI.SSI_Created_Date__c}" label="{!$Label.SSI_Invoice_Start_Date}" />
            <apex:inputField value="{!toSSI.SSI_Created_Date__c}" label="{!$Label.SSI_Invoice_End_Date}"/>
            
            <apex:commandButton value="{!$Label.SSI_Dealer_Cleans}" style="margin-left:180px;" action="{!dealerClean}"/>

            </apex:pageBlockSection>
        </apex:pageBlock>
    </apex:form>
</apex:page>