function addFilter(){
    var camTp = document.getElementById("cmptp");
    var cat = document.getElementById("catg");
    var tp = document.getElementById("tp");
    var cct = document.getElementById("cct");
    
    switch (camTp.value)
    {
        case "Central Marketing Campaign": 
            cat.style.display="none";
            tp.style.display="inline";
            cct.style.display="none";
            break;
        case "CAS Marketing Campaign":
            cat.style.display="none";
            tp.style.display="inline";
            cct.style.display="none";
            break;
        case "CAC CRM Campaign":
            cat.style.display="none";
            tp.style.display="none";
            cct.style.display="inline";
            break;
        case "CAC Campaign": 
            cat.style.display="inline";
            tp.style.display="none"; 
            cct.style.display="none";
            break; 
        default:
            cat.style.display="none";
            tp.style.display="none";
            cct.style.display="none";      
    }
}    

