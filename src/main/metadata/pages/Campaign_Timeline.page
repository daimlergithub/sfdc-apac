// Referesh Time line by Campaign Type
function refreshTimeline() {
    var partnerId = ("{!$CurrentPage.URL}").split("%3FpartnerId%3D").length == 1 ? "":("{!$CurrentPage.URL}").split("%3FpartnerId%3D")[1];
    var duration = document.getElementById("dur").value;
    var campaignType = document.getElementById("cmptp").value;
    var cat = document.getElementById("catsel").value;
    var tp = document.getElementById("tpsel").value;
    var cct = document.getElementById("cctsel").value;
    endYear = '';
    partnerId = '';
    theTimeLine.dispose();
    createBands();
    document.getElementById("timelineHeadline").innerHTML = "{!$Label.Campaign_Calendar_Page_All_Campaigns}";

    switch (duration) {
        case "this year on": 
            document.getElementById("timelineHeadline").innerHTML = "{!$Label.Campaign_Calendar_Page_All_Campaigns_From_This_Year_On}";
            currentYear = new Date().getFullYear();
            break;
        case "last year on": 
            document.getElementById("timelineHeadline").innerHTML = "{!$Label.Campaign_Calendar_Page_All_Campaigns_From_Last_Year_On}";
            currentYear = new Date().getFullYear() - 1;
            break;
        case "this year": 
            document.getElementById("timelineHeadline").innerHTML = "{!$Label.Campaign_Calendar_Page_All_Campaigns_During_This_Year}";
            currentYear = new Date().getFullYear();
            endYear = new Date().getFullYear();
            break;
        case "last year":
            document.getElementById("timelineHeadline").innerHTML = "{!$Label.Campaign_Calendar_Page_All_Campaigns_During_Last_Year}";
            currentYear = new Date().getFullYear() - 1;
            endYear = new Date().getFullYear() - 1;
            break;
        case "all":
            document.getElementById("timelineHeadline").innerHTML = "{!$Label.Campaign_Calendar_Page_All_Campaigns}";
            currentYear='';
            break;
        default: 
            document.getElementById("timelineHeadline").innerHTML = "{!$Label.Campaign_Calendar_Page_All_Campaigns}";
            currentYear='';
    }

    if(campaignType == "Central Marketing Campaign"){
        hiddeTd();
        document.getElementById("firstItem").innerHTML = "{!$Label.Campaign_Calendar_Page_CRM_Acquisition_Program}";
        document.getElementById("secondItem").innerHTML = "{!$Label.Campaign_Calendar_Page_Type_CRM_Loyalty_Program}";
        document.getElementById("thirdItem").innerHTML = "{!$Label.Campaign_Calendar_Page_Type_Other}";
        CPCalController.queCentralCamps(currentYear,endYear,partnerId,'',tp,function(result,event){layoutCentralCamp(result);});
        //CPCalController.queCentralCamps('','',partnerId,'','CRM Acquisition Program',function(result,event){layoutCentralCamp(result);});        
    } else if(campaignType == "CAS Marketing Campaign"){
        hiddeTd();
        document.getElementById("firstItem").innerHTML = "{!$Label.Campaign_Calendar_Page_CRM_Acquisition_Program}";
        document.getElementById("secondItem").innerHTML = "{!$Label.Campaign_Calendar_Page_Type_CRM_Loyalty_Program}";
        document.getElementById("thirdItem").innerHTML = "{!$Label.Campaign_Calendar_Page_Type_Other}";
        CPCalController.queCASCamps(currentYear,endYear, partnerId, tp,function(result,event){layoutCentralCamp(result);});  
    } else if(campaignType == "CAC CRM Campaign"){
        showTd();
        document.getElementById("firstItem").innerHTML = "{!$Label.Campaign_Calendar_Page_CAC_CRM_Type_Auto_Show}";
        document.getElementById("secondItem").innerHTML = "{!$Label.Campaign_Calendar_Page_CAC_CRM_Type_Finance}";
        document.getElementById("thirdItem").innerHTML = "{!$Label.Campaign_Calendar_Page_CAC_CRM_Type_Loyalty_Program}";
        document.getElementById("forthItem").innerHTML = "{!$Label.Campaign_Calendar_Page_CAC_CRM_Type_Competitions_and_Sales}";
        document.getElementById("fifthItem").innerHTML = "{!$Label.Campaign_Calendar_Page_CAC_CRM_Type_Interview}";
        document.getElementById("sixthItem").innerHTML = "{!$Label.Campaign_Calendar_Page_CAC_CRM_Type_Recall}";
        document.getElementById("seventhItem").innerHTML = "{!$Label.Campaign_Calendar_Page_CAC_CRM_Type_Product_Issue}";
        document.getElementById("eighthItem").innerHTML = "{!$Label.Campaign_Calendar_Page_CAC_CRM_Type_New_Car_Launch}";
        document.getElementById("ninthItem").innerHTML = "{!$Label.Campaign_Calendar_Page_CAC_CRM_Type_Others}";
        CPCalController.queCACCrmCamps(currentYear,endYear,partnerId, cct, function(result,event){layoutCentralCamp(result);});
    } else if(campaignType == "CAC Campaign"){
        //alert('currentYear' + currentYear);
        hiddeTd();
        document.getElementById("firstItem").innerHTML = "{!$Label.Campaign_Calendar_Page_Category_Ad_Hoc}";
        document.getElementById("secondItem").innerHTML = "{!$Label.Campaign_Calendar_Page_Category_Regular_Continuous}";
        document.getElementById("thirdItem").innerHTML = "{!$Label.Campaign_Calendar_Page_Category_Regular_Periodic}";
        CPCalController.queCACCamps(currentYear,endYear,partnerId,cat,function(result,event){layoutCACCamp(result);});
    }
}
