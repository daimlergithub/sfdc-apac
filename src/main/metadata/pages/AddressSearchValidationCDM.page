                                <apex:actionSupport event="onchange" reRender="personAccountPanelIN,personAccountPanelTR,companyAccountPanelIN,companyAccountPanelTR,addressIN,addressTR,buttonIN,buttonTR,mergeWarning"/>
                            </apex:selectList>
                        </apex:outputPanel>
