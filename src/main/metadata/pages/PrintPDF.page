<apex:page standardController="Lead__c" extensions="PrintPDF" renderAs="pdf" applyBodyTag="false" standardStylesheets="false"> 
   <!-- Stylesheets -->
   
     <head>      
      <style type="text/css" media="print">
       body { font-family: 'Arial Unicode MS';    
       }
       @page { 
        size: A4;
                margin-top:15mm;
                margin-left:10mm;
                margin-right:10mm;
                margin-bottom:15mm;                
                padding-top: 10px;
        @top-left {
        content: element(header_left);
        }
        @bottom-right {
            content: element(footer_right);
        }      
        @bottom-center {
            content: element(footer_center);
        }      
        }    
        .header_left {
            display: block;
            text-align: left;
            position: running(header_left);
        }     
       .footer_right {
            display: block;
            padding: 5px;
            text-align: right;
            position: running(footer_right);
        }      
       .footer_center {
            display: block;
            padding: 5px;
            text-align: center;
            position: running(footer_center);
        }      
        .pageNumber:before {
            content: counter(page); 
        }
        
        .pageCount:before {
            content: counter(pages);  
        }
        table,tr,td {            
            border-collapse:collapse;
            }
        table,td{    
            border: 1px solid black;  
            font-size:12px;
           }
        td{
            padding-left:1px;
        }
        .TopTd{
            font-size:14px;
        }
        .TopTdLabel{
            font-size:14px;
            font-weight: bold;
        }       
        td.TdLabel{
          font-weight: bold;
        }
        table{
            width: 702px;
            font-family: Arial Unicode MS;          
           -fs-table-paginate: paginate;  
          
          }
         
          thead{display: table-header-group;}
         .Table
           {
             display:table;
             margin-left:8px;
             width: 702px;
             padding-top:14px;
           }
            .Row
            {
              display: table-row;
            }
         .Cell
            {
              display: table-cell;
              border: solid;
              border-width: 3px;
              padding-left:1px;
              border-collapse:collapse;
              font-size:12px; 
              font-family: Arial Unicode MS;  
              font-weight: bold;            
            }                
         .CellLabel
            {
              display: table-cell;
              border: solid;
              border-width: 3px;
              padding-left:1px;
              border-collapse:collapse;
              font-size:12px; 
              font-family: Arial Unicode MS;
              font-weight: bold;
            }                
         .CellTopLeft
            {
              display: table-cell;
              border: solid;
              border-width: 3px;
              border-collapse:collapse;
              font-size:18px; 
              font-family: Arial Unicode MS;
              width:180px;
              text-align:center;
            }                
         .CellTopRight
            {
              display: table-cell;
              border: solid;
              border-width: 3px;
              border-collapse:collapse;
              font-size:18px; 
              font-family: Arial Unicode MS;              
              width:50px;
              text-align:center;
            } 
            
                   
       </style>
    </head>  
