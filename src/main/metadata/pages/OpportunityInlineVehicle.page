<!-- Inline VF page added to show Vehicle in Opportunity page;  Lead2Opty -->														 
<apex:page standardController="Opportunity" extensions="OpportunityInlineVehicleController" lightningStylesheets="true">
  <apex:form >
      <apex:pageBlock rendered="{!IF($Permission.INGeneric || $Permission.TRGeneric || $Permission.AUGeneric|| $Permission.NZGeneric , true, false)}">
          <!-- Interested Vehicle -->
          <apex:pageBlockSection title="{!$Label.IntVehicle}" columns="1" rendered="{!IF($Permission.INGeneric || $Permission.TRGeneric, true, false)}" >
                <apex:pageBlockTable id="pb1" value="{!allIntVehicles}" var="intveh" style="width:900px;" >
                    <apex:column headerValue="{!$Label.CarModel}">
                         <apex:outputLink value="/{!intveh.Car_Model__c}" target="_blank" style="width:7%; white-space:none;">
                            {!intveh.Car_Model__r.Name}
                        </apex:outputLink>
                    </apex:column>
                    <apex:column headerValue="{!$Label.Brand}" rendered="{!IF($Permission.INGeneric , false, true)}">
                        <apex:outputField value="{!intveh.Brand__c}" style="width:7%; white-space:none;">
                        </apex:outputField>
                    </apex:column>
                    <apex:column headerValue="{!$Label.Class}" rendered="{!IF($Permission.INGeneric , false, true)}">
                        <apex:outputField value="{!intveh.Class__c}" style="width:7%; white-space:none;">
                        </apex:outputField>
                    </apex:column>
                    <apex:column headerValue="{!$Label.Brand}" rendered="{!IF($Permission.INGeneric , true,false)}">
                        <apex:outputField value="{!intveh.Brand_Picklist__c}" style="width:7%; white-space:none;">
                        </apex:outputField>
                    </apex:column>
                    <apex:column headerValue="{!$Label.Class}" rendered="{!IF($Permission.INGeneric , true,false)}">
                        <apex:outputField value="{!intveh.Class_Picklist__c}" style="width:7%; white-space:none;">
                        </apex:outputField>
                    </apex:column>
                    <apex:column headerValue="{!$Label.Color}">
                        <apex:outputField value="{!intveh.Color__c}" style="width:7%; white-space:none;">
                        </apex:outputField>
                    </apex:column>
                    <apex:column headerValue="{!$Label.Preferred}">
                        <apex:outputField value="{!intveh.Preferred__c}" style="width:7%; white-space:none;">
                        </apex:outputField>
                    </apex:column>
                    <apex:column headerValue="{!$Label.CarModelTxt}" rendered="{!IF($Permission.TRGeneric , true,false)}">
                        <apex:outputField value="{!intveh.Car_Model_Text__c}" style="width:7%; white-space:none;">
                        </apex:outputField>
                    </apex:column>
                </apex:pageBlockTable>       
            </apex:pageBlockSection>
     	
          <!-- Interested Vehicle -->
          <apex:pageBlockSection title="Interested Vehicle Information" columns="1"  rendered="{!IF(($Permission.AUGeneric || $Permission.NZGeneric), true, false)}">
                <apex:pageBlockTable id="pb1" value="{!allIntVehicles}" var="intveh" style="width:900px;" >
                    <apex:column headerValue="Car Model">
                        <apex:outputField value="{!intveh.Car_Model__c}" style="width:7%; white-space:none;">
                        </apex:outputField>
                    </apex:column>
                    <apex:column headerValue="Marketing Class">
                        <apex:outputField value="{!intveh.Car_Model__r.Marketing_Class__c}" style="width:7%; white-space:none;">
                        </apex:outputField>
                    </apex:column>
                    <apex:column headerValue="Body Type">
                        <apex:outputField value="{!intveh.Car_Model__r.Marketing_Body_Type__c}" style="width:7%; white-space:none;">
                        </apex:outputField>
                    </apex:column>
                    <apex:column headerValue="Variant">
                        <apex:outputField value="{!intveh.Car_Model__r.Variant__c}" style="width:7%; white-space:none;">
                        </apex:outputField>
                    </apex:column>
                    <apex:column headerValue="Fuel Type">
                        <apex:outputField value="{!intveh.Car_Model__r.Fuel_Type__c}" style="width:7%; white-space:none;">
                        </apex:outputField>
                    </apex:column>
                </apex:pageBlockTable>       
            </apex:pageBlockSection>
            
            <!-- Competetor Vehicle -->
            <apex:pageBlockSection title="{!$Label.CompVehicle}" columns="1" >
                <apex:pageBlockTable id="pb2" value="{!allCompVehicles}" var="compveh" style="width:900px;" >
                    <apex:column headerValue="{!$Label.CarModel}">
                        <apex:outputField value="{!compveh.Car_Model_Competitor__c}" style="width:7%; white-space:none;">
                        </apex:outputField>
                    </apex:column>
                    <apex:column headerValue="{!$Label.Brand}" rendered="{!IF($Permission.INGeneric ||  $Permission.TRGeneric, false,true)}">
                        <apex:outputField value="{!compveh.Brand__c}" style="width:7%; white-space:none;">
                        </apex:outputField>
                    </apex:column>
                    <apex:column headerValue="{!$Label.Class}" rendered="{!IF($Permission.INGeneric ||  $Permission.TRGeneric, false,true)}">
                        <apex:outputField value="{!compveh.Class__c}" style="width:7%; white-space:none;">
                        </apex:outputField>
                    </apex:column>
                    <apex:column headerValue="{!$Label.Brand}" rendered="{!IF($Permission.INGeneric ||  $Permission.TRGeneric, true,false)}">
                        <apex:outputField value="{!compveh.Brand_Picklist__c}" style="width:7%; white-space:none;">
                        </apex:outputField>
                    </apex:column>
                    <apex:column headerValue="{!$Label.Class}" rendered="{!IF($Permission.INGeneric ||  $Permission.TRGeneric, true,false)}">
                        <apex:outputField value="{!compveh.Class_Picklist__c}" style="width:7%; white-space:none;">
                        </apex:outputField>
                    </apex:column>
                    <apex:column headerValue="{!$Label.Color}">
                        <apex:outputField value="{!compveh.Color__c}" style="width:7%; white-space:none;">
                        </apex:outputField>
                    </apex:column>
                    <apex:column headerValue="{!$Label.Preferred}">
                        <apex:outputField value="{!compveh.Preferred__c}" style="width:7%; white-space:none;">
                        </apex:outputField>
                    </apex:column>
                </apex:pageBlockTable>       
            </apex:pageBlockSection>
            
            <!-- Trade In Vehicle -->
            <apex:pageBlockSection title="{!$Label.TradeIn}" columns="1" >
                <apex:pageBlockTable id="pb3" value="{!allTradeVehicles}" var="tradeveh" style="width:900px;" >
                    <apex:column headerValue="{!$Label.TradeInVeh}">
                        <apex:outputField value="{!tradeveh.Trade_In_Vehicle__c}" style="width:7%; white-space:none;">
                        </apex:outputField>
                    </apex:column>
                    <apex:column headerValue="{!$Label.Year_Manufacture}">
                        <apex:outputField value="{!tradeveh.Year_of_Manufacture__c}" style="width:7%; white-space:none;">
                        </apex:outputField>
                    </apex:column>
                    <apex:column headerValue="{!$Label.Mileage}">
                        <apex:outputField value="{!tradeveh.Mileage__c}" style="width:7%; white-space:none;">
                        </apex:outputField>
                    </apex:column>
                    <apex:column headerValue="{!$Label.Trim}">
                        <apex:outputField value="{!tradeveh.Trim__c}" style="width:7%; white-space:none;">
                        </apex:outputField>
                    </apex:column>
                </apex:pageBlockTable>       
            </apex:pageBlockSection>
            
            <!-- Current Vehicle -->
           <apex:pageBlockSection title="{!$Label.CurrVehicle}" columns="1" >
                <apex:pageBlockTable id="pb4" value="{!currentVehicleList}"  var="currveh" style="width:900px;" >
                    <apex:column headerValue="{!$Label.CarModel}">
                        <apex:outputLabel value="{!currveh.Vehicle_Model__c}" />
                    </apex:column>
                    <apex:column headerValue="{!$Label.Brand}">
                        <apex:outputLabel value="{!currveh.Vehicle_Brand__c}" />
                    </apex:column>
                    <apex:column headerValue="{!$Label.Class}">
                        <apex:outputLabel value="{!currveh.Vehicle_Class__c}" />
                    </apex:column>
                    <apex:column headerValue="{!$Label.Mileage_Start}">
                        <apex:outputLabel value="{!currveh.Mileage_start__c}" />
                    </apex:column>
                    <apex:column headerValue="{!$Label.Mileage}">
                        <apex:outputLabel value="{!currveh.Mileage_End__c}" />
                    </apex:column>
                </apex:pageBlockTable>        
            </apex:pageBlockSection>
      </apex:pageBlock>
  </apex:form>
</apex:page>