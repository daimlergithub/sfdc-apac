<apex:page standardController="Account" extensions="ConsentHistoryInlinepage_TH_Ctrl" showHeader="false" sidebar="false">
    <apex:form >
        <apex:pageblock rendered="{!IF(fieldHistoryList.size=0,true,false)}">
            <br/>There is no Consent History Data present for this Account.
        </apex:pageblock>
        <apex:pageblock mode="maindetail" rendered="{!IF(fieldHistoryList.size=0,false,true)}">
            <apex:pageblockSection columns="1">
                <apex:pageblockTable value="{!fieldHistoryList}" var="fieldHist">
                    <apex:column value="{!fieldHist.Personal_Agreement__c}"/>
                    <apex:column value="{!fieldHist.Lastest_Consent_Date__c}"/>
                    <apex:column value="{!fieldHist.Updated_Consent_User__c}"/>
                    <apex:column value="{!fieldHist.User_Organization__c}"/>
                    <apex:column value="{!fieldHist.Updated_Consent_Date__c}"/>
                    <apex:column value="{!fieldHist.System_Data_Source__c}"/>
                </apex:pageblockTable>
            </apex:pageblockSection>
        </apex:pageblock>
    </apex:form>
</apex:page>