<?xml version="1.0" encoding="UTF-8"?>
<transformations>
	<!-- in this org the customer portal is enabled, but we do not want this deprecated feature in the metadata -->
	<removeelement deploy="false">
		<filename>objects\Account.object</filename>
		<xpath>/CustomObject/fields[fullName/text()='IsCustomerPortal']</xpath>
	</removeelement>
	<removeelement deploy="false">
		<filename>objects\Contact.object</filename>
		<xpath>/CustomObject/fields[fullName/text()='CanAllowPortalSelfReg']</xpath>
	</removeelement>
	<removeelement deploy="false">
		<filename>profiles\*.profile</filename>
		<xpath>/Profile/fieldPermissions[field/text()='Contact.CanAllowPortalSelfReg']</xpath>
	</removeelement>
</transformations>
